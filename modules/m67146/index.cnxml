            <row>
              <entry align="left">Wypadkowa szybkość wymiany ciepła przez promieniowanie</entry>
              <entry align="left">
                <m:math display="inline">
                  <m:msub>
                    <m:mi>P</m:mi>
                    <m:mtext>wyp</m:mtext>
                  </m:msub>
                  <m:mo>=</m:mo>
                  <m:mrow>
                    <m:mi>σ</m:mi>
                    <m:mo>⁢</m:mo>
                    <m:mi>e</m:mi>
                    <m:mo>⁢</m:mo>
                    <m:mi>S</m:mi>
