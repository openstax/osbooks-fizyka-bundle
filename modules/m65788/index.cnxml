                                        <m:mo>,</m:mo>
                                      </m:mtd>
                                    </m:mtr>
                                    <m:mtr>
                                      <m:mtd>
                                        <m:msub>
                                          <m:mrow>
                                            <m:mover>
                                              <m:mi>r</m:mi>
                                              <m:mtext>→</m:mtext>
                                            </m:mover>
                                          </m:mrow>
                                          <m:mn>2</m:mn>
                                        </m:msub>
