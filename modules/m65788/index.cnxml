                                    </m:mtr>
                                    <m:mtr>
                                      <m:mtd>
                                        <m:msub>
                                          <m:mrow>
                                            <m:mover>
                                              <m:mi>r</m:mi>
                                              <m:mtext>→</m:mtext>
                                            </m:mover>
                                          </m:mrow>
                                          <m:mn>4</m:mn>
                                        </m:msub>
                                      </m:mtd>
                                      <m:mtd>
                                        <m:mi/>
                                        <m:mo>=</m:mo>
                                        <m:mn>2,36</m:mn>
                                        <m:mo>⋅</m:mo>
                                        <m:msup>
                                          <m:mn>10</m:mn>
                                          <m:mrow>
                                            <m:mo>−</m:mo>
                                            <m:mn>10</m:mn>
                                          </m:mrow>
                                        </m:msup>
                                        <m:mspace width="0.2em"/>
                                        <m:mrow>
