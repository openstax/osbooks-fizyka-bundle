                                            <m:mi>x</m:mi>
                                          </m:mrow>
                                        </m:msub>
                                        <m:mrow>
                                          <m:mover>
                                            <m:mi>i</m:mi>
                                            <m:mo stretchy="false">^</m:mo>
                                          </m:mover>
                                        </m:mrow>
                                        <m:mo>+</m:mo>
                                        <m:msub>
                                          <m:mi>r</m:mi>
                                          <m:mrow>
                                            <m:mn>3</m:mn>
                                            <m:mi>y</m:mi>
                                          </m:mrow>
                                        </m:msub>
                                        <m:mrow>
                                          <m:mover>
                                            <m:mi>j</m:mi>
                                            <m:mo stretchy="false">^</m:mo>
                                          </m:mover>
                                        </m:mrow>
                                        <m:mo>=</m:mo>
                                        <m:mn>2,36</m:mn>
                                        <m:mo>⋅</m:mo>
                                        <m:msup>
                                          <m:mn>10</m:mn>
                                          <m:mrow>
                                            <m:mo>−</m:mo>
                                            <m:mn>10</m:mn>
                                          </m:mrow>
                                        </m:msup>
                                        <m:mspace width="0.2em"/>
                                        <m:mrow>
                                          <m:mi mathvariant="normal">m</m:mi>
                                        </m:mrow>
                                        <m:mo>⋅</m:mo>
