                                        <m:mrow>
                                          <m:mi mathvariant="normal">m</m:mi>
                                        </m:mrow>
                                        <m:mo>⋅</m:mo>
                                        <m:mrow>
                                          <m:mover>
                                            <m:mi>i</m:mi>
