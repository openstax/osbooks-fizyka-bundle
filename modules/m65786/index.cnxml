                                                <m:mspace width="0.2em"/>
                                                <m:mrow>
                                                  <m:mi mathvariant="normal">k</m:mi>
                                                  <m:mi mathvariant="normal">g</m:mi>
                                                </m:mrow>
                                                </m:math>.</item>
                                                <item>Energia kinetyczna układu, podobnie jak i pęd, pochodzi w całości z ruchu rozpędzonego młota:<newline/>
                                                  <equation id="fs-id1167131428975" class="unnumbered">
                                                    <label/>
                                                    <m:math>
                                                      <m:msub>
                                                        <m:mi>E</m:mi>
                                                        <m:mrow>
                                                          <m:mi>K</m:mi>
                                                          <m:mtext> przed</m:mtext>
                                                        </m:mrow>
                                                      </m:msub>
                                                      <m:mo>=</m:mo>
                                                      <m:mfrac>
                                                        <m:mn>1</m:mn>
                                                        <m:mn>2</m:mn>
                                                      </m:mfrac>
                                                      <m:msub>
                                                        <m:mi>M</m:mi>
                                                        <m:mtext>m</m:mtext>
                                                      </m:msub>
                                                      <m:msubsup>
                                                        <m:mi>v</m:mi>
                                                        <m:mtext>m</m:mtext>
                                                        <m:mn>2</m:mn>
                                                      </m:msubsup>
                                                      <m:mo>=</m:mo>
                                                      <m:mfrac>
                                                        <m:mn>1</m:mn>
                                                        <m:mn>2</m:mn>
                                                      </m:mfrac>
                                                      <m:mo>⋅</m:mo>
                                                      <m:mn>70</m:mn>
                                                      <m:mspace width="0.2em"/>
                                                      <m:mrow>
                                                        <m:mi mathvariant="normal">k</m:mi>
                                                        <m:mi mathvariant="normal">g</m:mi>
                                                      </m:mrow>
                                                      <m:mo>⋅</m:mo>
                                                      <m:msup>
                                                        <m:mrow>
                                                          <m:mo>(</m:mo>
                                                          <m:mrow>
                                                            <m:mn>10</m:mn>
                                                            <m:mspace width="0.2em"/>
                                                            <m:mrow>
                                                              <m:mi mathvariant="normal">m</m:mi>
                                                              <m:mspace width="0.1em"/><m:mtext>/</m:mtext><m:mspace width="0.1em"/>
                                                              <m:mi mathvariant="normal">s</m:mi>
                                                            </m:mrow>
                                                          </m:mrow>
                                                          <m:mo>)</m:mo>
                                                        </m:mrow>
                                                        <m:mn>2</m:mn>
                                                      </m:msup>
