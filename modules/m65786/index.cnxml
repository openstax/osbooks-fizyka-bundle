                                                            <m:mo>)</m:mo>
                                                          </m:mrow>
                                                        </m:mtd>
                                                        <m:mtd>
                                                          <m:mi/>
                                                          <m:mo>=</m:mo>
                                                          <m:msub>
                                                            <m:mi>M</m:mi>
                                                            <m:mtext>I</m:mtext>
                                                          </m:msub>
                                                          <m:mi>v</m:mi>
                                                          <m:mo>,</m:mo>
                                                        </m:mtd>
                                                      </m:mtr>
                                                      <m:mtr>
                                                        <m:mtd>
                                                          <m:msub>
                                                            <m:mi>M</m:mi>
                                                            <m:mtext>m</m:mtext>
                                                          </m:msub>
                                                        </m:mtd>
                                                        <m:mtd>
                                                          <m:mi/>
                                                          <m:mo>=</m:mo>
                                                          <m:mfrac>
                                                            <m:mrow>
                                                              <m:msub>
                                                                <m:mi>M</m:mi>
                                                                <m:mtext>I</m:mtext>
                                                              </m:msub>
                                                              <m:mi>v</m:mi>
                                                            </m:mrow>
                                                            <m:mrow>
                                                              <m:msub>
                                                                <m:mi>v</m:mi>
                                                                <m:mtext>m</m:mtext>
                                                              </m:msub>
                                                              <m:mo>−</m:mo>
                                                              <m:mi>v</m:mi>
                                                            </m:mrow>
                                                          </m:mfrac>
                                                          <m:mo>=</m:mo>
                                                          <m:mfrac>
                                                            <m:mrow>
                                                              <m:mn>200</m:mn>
                                                              <m:mspace width="0.2em"/>
                                                              <m:mrow>
                                                                <m:mi mathvariant="normal">k</m:mi>
                                                                <m:mi mathvariant="normal">g</m:mi>
                                                              </m:mrow>
                                                              <m:mo>⋅</m:mo>
                                                              <m:mfrac>
                                                                <m:mrow>
                                                                  <m:mn>2</m:mn>
                                                                  <m:mspace width="0.2em"/>
                                                                  <m:mrow>
                                                                    <m:mi mathvariant="normal">m</m:mi>
                                                                  </m:mrow>
                                                                </m:mrow>
                                                                <m:mrow>
                                                                  <m:mn>0,75</m:mn>
                                                                  <m:mspace width="0.2em"/>
                                                                  <m:mrow>
                                                                    <m:mi mathvariant="normal">s</m:mi>
                                                                  </m:mrow>
                                                                </m:mrow>
