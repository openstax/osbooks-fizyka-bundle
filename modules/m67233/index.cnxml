    <m:mo>=</m:mo>
    <m:mrow>
      <m:mo>arc tg</m:mo>
<m:mfenced>
      <m:mfrac>
        <m:mrow>
          <m:mn>2,77</m:mn>
          <m:mo rspace="verythinmathspace" lspace="">⁢</m:mo>
