          <m:mi mathvariant="normal">%</m:mi>
        </m:mrow>
      </m:mrow>
      <m:mo id="U10314ed6-be77-43eb-802b-ae9c86c3e99f-copy" indenttarget="Ue116fa9a-b42f-4d3a-86ee-b664ae7dc8c1" linebreak="newline" indentalign="id">=</m:mo>
      <m:mrow>
        <m:mfrac>
          <m:mrow>
            <m:mn>1705</m:mn>
            <m:mo rspace="veryverythinmathspace" lspace="veryverythinmathspace">⁢</m:mo>
            <m:mi mathvariant="normal">Pa</m:mi>
          </m:mrow>
          <m:mrow>
            <m:mn>3167</m:mn>
            <m:mo rspace="veryverythinmathspace" lspace="veryverythinmathspace">⁢</m:mo>
            <m:mi mathvariant="normal">Pa</m:mi>
          </m:mrow>
        </m:mfrac>
        <m:mo>⋅</m:mo>
        <m:mrow>
          <m:mn>100</m:mn>
          <m:mo rspace="veryverythinmathspace" lspace="veryverythinmathspace">⁢</m:mo>
          <m:mi mathvariant="normal">%</m:mi>
        </m:mrow>
      </m:mrow>
      <m:mo>=</m:mo>
      <m:mrow>
        <m:mrow>
          <m:mn>53,8</m:mn>
          <m:mo rspace="veryverythinmathspace" lspace="veryverythinmathspace">⁢</m:mo>
