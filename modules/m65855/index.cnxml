                </m:msup>
                <m:mo>⁢</m:mo>
                <m:mfenced>
                  <m:mtable>
                    <m:mtr>
                      <m:mtd>
                        <m:mrow>
                          <m:mi>k</m:mi>
                          <m:mo>+</m:mo>
                          <m:mi>n</m:mi>
                        </m:mrow>
                        <m:mo>−</m:mo>
                        <m:mn>1</m:mn>
                      </m:mtd>
                    </m:mtr>
                    <m:mtr>
                      <m:mtd>
                        <m:mi>k</m:mi>
                      </m:mtd>
                    </m:mtr>
                  </m:mtable>
                </m:mfenced>
                <m:mo>⁢</m:mo>
                <m:msup>
                  <m:mi>x</m:mi>
                  <m:mi>k</m:mi>
                </m:msup>
                <m:mo>⁢</m:mo>
                <m:mtext>, dla </m:mtext>
                <m:mo>⁢</m:mo>
                <m:mfenced close="|" open="|">
                  <m:mi>x</m:mi>
                </m:mfenced>
              </m:mrow>
            </m:mrow>
          </m:mrow>
          <m:mo>&lt;</m:mo>
          <m:mn>1</m:mn>
        </m:mrow>
        <m:annotation encoding="application/vnd.katalysteducation.mathtex"> (1\pm x)^{-n} = 1 \mp \frac{nx}{1!} + \frac{n(n+1)x^2}{2!} \mp \dots = \sum_{k=0}^{\infty} ( \prefop{\mp} 1)^k \begin{pmatrix} k+n-1 \\ k \end{pmatrix} x^k \text{, dla } \abs{x} &lt; 1</m:annotation>
      </m:semantics>
    </m:math>
  </item>
  <item>
