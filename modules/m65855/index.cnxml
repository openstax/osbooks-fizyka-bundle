<para id="fs-id1171241190802">
  <title>Rozwinięcie funkcji w szeregi potęgowe</title>
</para>
<list id="fs-id1171241016910" list-type="enumerated" number-style="arabic">
  <item>Wzór dwumianowy:<newline/>
    <m:math display="inline" displaystyle="true">
      <m:semantics>
        <m:mrow>
          <m:msup>
            <m:mfenced>
              <m:mrow>
                <m:mi>a</m:mi>
                <m:mo>+</m:mo>
                <m:mi>b</m:mi>
              </m:mrow>
            </m:mfenced>
            <m:mi>n</m:mi>
          </m:msup>
          <m:mo id="Uef2b112d-f09d-4ccb-bd52-85e722d96537">=</m:mo>
          <m:mrow>
            <m:msup>
              <m:mi>a</m:mi>
              <m:mi>n</m:mi>
            </m:msup>
            <m:mo>+</m:mo>
            <m:mrow>
              <m:mfrac>
