                </m:mrow>
              </m:msup>
              <m:mo>⁢</m:mo>
              <m:mi>b</m:mi>
            </m:mrow>
            <m:mo>+</m:mo>
            <m:mrow>
              <m:mfrac>
                <m:mrow>
                  <m:mi>n</m:mi>
                  <m:mo>⁢</m:mo>
                  <m:mfenced>
                    <m:mrow>
                      <m:mi>n</m:mi>
                      <m:mo>−</m:mo>
                      <m:mn>1</m:mn>
                    </m:mrow>
                  </m:mfenced>
                </m:mrow>
                <m:mrow>
                  <m:mn>2</m:mn>
                  <m:mo>!</m:mo>
                </m:mrow>
              </m:mfrac>
              <m:mo>⁢</m:mo>
              <m:msup>
                <m:mi>a</m:mi>
                <m:mrow>
                  <m:mi>n</m:mi>
                  <m:mo>−</m:mo>
                  <m:mn>2</m:mn>
                </m:mrow>
              </m:msup>
              <m:mo>⁢</m:mo>
              <m:msup>
                <m:mi>b</m:mi>
                <m:mn>2</m:mn>
              </m:msup>
            </m:mrow>
            <m:mo>+</m:mo>
            <m:mi>…</m:mi>
            <m:mo>+</m:mo>
            <m:mrow>
              <m:mfrac>
                <m:mrow>
                  <m:mi>n</m:mi>
                  <m:mo>⁢</m:mo>
                  <m:mfenced>
                    <m:mrow>
                      <m:mi>n</m:mi>
                      <m:mo>−</m:mo>
                      <m:mn>1</m:mn>
                    </m:mrow>
                  </m:mfenced>
                </m:mrow>
                <m:mrow>
                  <m:mn>2</m:mn>
                  <m:mo>!</m:mo>
                </m:mrow>
              </m:mfrac>
              <m:mo>⁢</m:mo>
              <m:msup>
                <m:mi>a</m:mi>
                <m:mn>2</m:mn>
              </m:msup>
              <m:mo>⁢</m:mo>
              <m:msup>
                <m:mi>b</m:mi>
                <m:mrow>
                  <m:mi>n</m:mi>
                  <m:mo>−</m:mo>
                  <m:mn>2</m:mn>
                </m:mrow>
              </m:msup>
            </m:mrow>
            <m:mo>+</m:mo>
            <m:mrow>
              <m:mfrac>
                <m:mi>n</m:mi>
                <m:mrow>
                  <m:mn>1</m:mn>
                  <m:mo>!</m:mo>
                </m:mrow>
              </m:mfrac>
              <m:mo>⁢</m:mo>
              <m:mi>a</m:mi>
              <m:mo>⁢</m:mo>
              <m:msup>
                <m:mi>b</m:mi>
                <m:mrow>
                  <m:mi>n</m:mi>
                  <m:mo>−</m:mo>
                  <m:mn>1</m:mn>
                </m:mrow>
              </m:msup>
            </m:mrow>
            <m:mo>+</m:mo>
            <m:msup>
              <m:mi>b</m:mi>
              <m:mi>n</m:mi>
            </m:msup>
          </m:mrow>
          <m:mo indenttarget="Uef2b112d-f09d-4ccb-bd52-85e722d96537" indentalign="id" id="Ue66c4f26-a8f5-4d20-84d6-4d469946ae4e" linebreak="newline">=</m:mo>
          <m:mrow>
            <m:munderover>
              <m:mo>∑</m:mo>
              <m:mrow>
                <m:mi>k</m:mi>
                <m:mo>=</m:mo>
                <m:mn>0</m:mn>
              </m:mrow>
              <m:mi>n</m:mi>
            </m:munderover>
            <m:mrow>
              <m:mfenced>
                <m:mtable>
                  <m:mtr>
                    <m:mtd>
                      <m:mi>n</m:mi>
                    </m:mtd>
                  </m:mtr>
                  <m:mtr>
                    <m:mtd>
                      <m:mi>k</m:mi>
                    </m:mtd>
                  </m:mtr>
                </m:mtable>
              </m:mfenced>
              <m:mo>⁢</m:mo>
              <m:msup>
                <m:mi>a</m:mi>
                <m:mrow>
                  <m:mi>n</m:mi>
                  <m:mo>−</m:mo>
                  <m:mi>k</m:mi>
                </m:mrow>
              </m:msup>
              <m:mo>⁢</m:mo>
              <m:msup>
                <m:mi>b</m:mi>
                <m:mi>k</m:mi>
              </m:msup>
            </m:mrow>
          </m:mrow>
        </m:mrow>
        <m:annotation encoding="application/vnd.katalysteducation.mathtex"> \begin{multiline} (a+b)^n &amp;= a^n + \frac{n}{1!} a^{n-1} b + \frac{n (n-1)}{2!} a^{n-2} b^2 + \dots + \frac{n(n-1)}{2!} a^2 b^{n-2} + \frac{n}{1!} a b^{n-1} + b^n \\ &amp;= \sum_{k=0}^n \begin{pmatrix} n \\ k \end{pmatrix} a^{n-k}b^{k}\end{multiline}</m:annotation>
      </m:semantics>
    </m:math>
  </item>
  <item>
    <m:math display="inline" displaystyle="true">
      <m:semantics>
        <m:mrow>
          <m:mrow>
            <m:msup>
              <m:mfenced>
                <m:mrow>
                  <m:mn>1</m:mn>
                  <m:mo>±</m:mo>
                  <m:mi>x</m:mi>
                </m:mrow>
              </m:mfenced>
              <m:mi>n</m:mi>
            </m:msup>
            <m:mo>=</m:mo>
            <m:mrow>
              <m:mrow>
                <m:mrow>
                  <m:mn>1</m:mn>
                  <m:mo>±</m:mo>
                  <m:mfrac>
                    <m:mrow>
                      <m:mi>n</m:mi>
                      <m:mo>⁢</m:mo>
                      <m:mi>x</m:mi>
                    </m:mrow>
                    <m:mrow>
                      <m:mn>1</m:mn>
                      <m:mo>!</m:mo>
                    </m:mrow>
                  </m:mfrac>
                </m:mrow>
                <m:mo>+</m:mo>
                <m:mfrac>
                  <m:mrow>
                    <m:mi>n</m:mi>
                    <m:mo>⁢</m:mo>
                    <m:mfenced>
                      <m:mrow>
                        <m:mi>n</m:mi>
                        <m:mo>−</m:mo>
                        <m:mn>1</m:mn>
                      </m:mrow>
                    </m:mfenced>
                    <m:mo>⁢</m:mo>
                    <m:msup>
                      <m:mi>x</m:mi>
                      <m:mn>2</m:mn>
                    </m:msup>
                  </m:mrow>
                  <m:mrow>
                    <m:mn>2</m:mn>
                    <m:mo>!</m:mo>
                  </m:mrow>
                </m:mfrac>
              </m:mrow>
              <m:mo>±</m:mo>
              <m:mi>…</m:mi>
            </m:mrow>
            <m:mo>=</m:mo>
            <m:mrow>
              <m:munderover>
                <m:mo>∑</m:mo>
                <m:mrow>
                  <m:mi>k</m:mi>
                  <m:mo>=</m:mo>
                  <m:mn>0</m:mn>
                </m:mrow>
                <m:mi>n</m:mi>
              </m:munderover>
              <m:mrow>
                <m:msup>
                  <m:mfenced>
                    <m:mrow>
                      <m:mo>±</m:mo>
                      <m:mn>1</m:mn>
                    </m:mrow>
                  </m:mfenced>
                  <m:mi>k</m:mi>
                </m:msup>
                <m:mo>⁢</m:mo>
                <m:mfenced>
                  <m:mtable>
                    <m:mtr>
                      <m:mtd>
                        <m:mi>n</m:mi>
                      </m:mtd>
                    </m:mtr>
                    <m:mtr>
                      <m:mtd>
                        <m:mi>k</m:mi>
                      </m:mtd>
                    </m:mtr>
                  </m:mtable>
                </m:mfenced>
                <m:mo>⁢</m:mo>
                <m:msup>
                  <m:mi>x</m:mi>
                  <m:mi>k</m:mi>
                </m:msup>
                <m:mo>⁢</m:mo>
                <m:mtext>, dla </m:mtext>
                <m:mo>⁢</m:mo>
                <m:mfenced close="|" open="|">
                  <m:mi>x</m:mi>
                </m:mfenced>
              </m:mrow>
            </m:mrow>
          </m:mrow>
          <m:mo>&lt;</m:mo>
          <m:mn>1</m:mn>
        </m:mrow>
        <m:annotation encoding="application/vnd.katalysteducation.mathtex"> (1\pm x)^n = 1 \pm \frac{nx}{1!} + \frac{n(n-1)x^2}{2!} \pm \dots = \sum_{k=0}^n ( \prefop{\pm} 1)^k \begin{pmatrix} n \\ k \end{pmatrix} x^k \text{, dla } \abs{x} &lt; 1</m:annotation>
      </m:semantics>
    </m:math>
  </item>
  <item>
    <m:math display="inline" displaystyle="true">
      <m:semantics>
        <m:mrow>
          <m:mrow>
            <m:msup>
              <m:mfenced>
                <m:mrow>
                  <m:mn>1</m:mn>
                  <m:mo>±</m:mo>
                  <m:mi>x</m:mi>
                </m:mrow>
              </m:mfenced>
              <m:mrow>
                <m:mo>−</m:mo>
                <m:mi>n</m:mi>
              </m:mrow>
            </m:msup>
            <m:mo>=</m:mo>
            <m:mrow>
              <m:mrow>
                <m:mrow>
                  <m:mn>1</m:mn>
                  <m:mo>∓</m:mo>
                  <m:mfrac>
                    <m:mrow>
                      <m:mi>n</m:mi>
                      <m:mo>⁢</m:mo>
                      <m:mi>x</m:mi>
                    </m:mrow>
                    <m:mrow>
                      <m:mn>1</m:mn>
                      <m:mo>!</m:mo>
                    </m:mrow>
                  </m:mfrac>
                </m:mrow>
                <m:mo>+</m:mo>
                <m:mfrac>
                  <m:mrow>
                    <m:mi>n</m:mi>
                    <m:mo>⁢</m:mo>
                    <m:mfenced>
                      <m:mrow>
                        <m:mi>n</m:mi>
                        <m:mo>+</m:mo>
                        <m:mn>1</m:mn>
                      </m:mrow>
                    </m:mfenced>
                    <m:mo>⁢</m:mo>
                    <m:msup>
