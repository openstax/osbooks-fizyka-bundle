                </m:mfrac>
              </m:mrow>
              <m:mo>∓</m:mo>
              <m:mi>…</m:mi>
            </m:mrow>
            <m:mo>=</m:mo>
            <m:mrow>
              <m:munderover>
                <m:mo>∑</m:mo>
                <m:mrow>
                  <m:mi>k</m:mi>
