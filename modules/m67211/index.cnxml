            </m:mfrac>
            <m:mo>−</m:mo>
            <m:mfrac>
              <m:mrow>
                <m:mrow>
                  <m:mrow>
                    <m:mrow>
                      <m:mn>4</m:mn>
                      <m:mo>⁢</m:mo>
                      <m:mi>π</m:mi>
                    </m:mrow>
                    <m:mo>⋅</m:mo>
                    <m:msup>
                      <m:mn>10</m:mn>
                      <m:mn>-7</m:mn>
                    </m:msup>
                  </m:mrow>
                  <m:mo>⁢</m:mo>
                  <m:mrow>
                    <m:mrow>
                      <m:mi mathvariant="normal">T</m:mi>
                      <m:mo rspace="veryverythinmathspace" lspace="veryverythinmathspace">⁢</m:mo>
                      <m:mi mathvariant="normal">m</m:mi>
                    </m:mrow>
                    <m:mo>∕</m:mo>
                    <m:mi mathvariant="normal">A</m:mi>
                  </m:mrow>
                </m:mrow>
                <m:mo>⋅</m:mo>
                <m:mrow>
                  <m:mn>0,01</m:mn>
                  <m:mo rspace="veryverythinmathspace" lspace="veryverythinmathspace">⁢</m:mo>
                  <m:mi mathvariant="normal">A</m:mi>
                </m:mrow>
                <m:mo>⋅</m:mo>
                <m:msup>
                  <m:mfenced>
                    <m:mrow>
                      <m:mn>1</m:mn>
                      <m:mo rspace="veryverythinmathspace" lspace="veryverythinmathspace">⁢</m:mo>
                      <m:mi mathvariant="normal">m</m:mi>
                    </m:mrow>
                  </m:mfenced>
                  <m:mn>2</m:mn>
                </m:msup>
              </m:mrow>
              <m:mrow>
                <m:mn>2</m:mn>
                <m:mo>⁢</m:mo>
                <m:msup>
                  <m:mfenced close="]" open="[">
                    <m:mrow>
                      <m:msup>
                        <m:mfenced>
                          <m:mrow>
                            <m:mn>0,75</m:mn>
                            <m:mo rspace="veryverythinmathspace" lspace="veryverythinmathspace">⁢</m:mo>
                            <m:mi mathvariant="normal">m</m:mi>
                          </m:mrow>
                        </m:mfenced>
                        <m:mn>2</m:mn>
                      </m:msup>
                      <m:mo>+</m:mo>
                      <m:msup>
                        <m:mfenced>
                          <m:mrow>
                            <m:mn>1</m:mn>
                            <m:mo rspace="veryverythinmathspace" lspace="veryverythinmathspace">⁢</m:mo>
                            <m:mi mathvariant="normal">m</m:mi>
                          </m:mrow>
                        </m:mfenced>
                        <m:mn>2</m:mn>
                      </m:msup>
                    </m:mrow>
                  </m:mfenced>
                  <m:mrow>
                    <m:mn>3</m:mn>
                    <m:mo>∕</m:mo>
                    <m:mn>2</m:mn>
                  </m:mrow>
                </m:msup>
              </m:mrow>
            </m:mfrac>
          </m:mrow>
        </m:mtd>
      </m:mtr>
