        <m:mrow>
          <m:mo>(</m:mo>
          <m:mrow>
            <m:mi>x</m:mi>
            <m:mo>,</m:mo>
            <m:mi>t</m:mi>
          </m:mrow>
          <m:mo>)</m:mo>
        </m:mrow>
        <m:mo>=</m:mo>
        <m:mn>3,00</m:mn>
        <m:mspace width="0.2em"/>
        <m:mtext>cm</m:mtext>
        <m:mspace width="0.2em"/>
        <m:mtext>sin</m:mtext>
        <m:mrow>
          <m:mo>(</m:mo>
          <m:mrow>
            <m:mn>6</m:mn>
            <m:mspace width="0.2em"/>
            <m:msup>
              <m:mrow>
                <m:mtext>m</m:mtext>
              </m:mrow>
              <m:mrow>
                <m:mn>−1</m:mn>
              </m:mrow>
            </m:msup>
            <m:mi>x</m:mi>
            <m:mo>+</m:mo>
            <m:mn>3</m:mn>
            <m:mspace width="0.2em"/>
            <m:msup>
              <m:mrow>
                <m:mtext>s</m:mtext>
              </m:mrow>
              <m:mrow>
                <m:mn>−1</m:mn>
              </m:mrow>
            </m:msup>
            <m:mi>t</m:mi>
          </m:mrow>
          <m:mo>)</m:mo>
        </m:mrow>
        <m:mo/>
      </m:mrow>
    </m:math> i <m:math>
    <m:mrow>
      <m:mi>y</m:mi>
      <m:mrow>
        <m:mo>(</m:mo>
        <m:mrow>
          <m:mi>x</m:mi>
          <m:mo>,</m:mo>
          <m:mi>t</m:mi>
        </m:mrow>
        <m:mo>)</m:mo>
      </m:mrow>
      <m:mo>=</m:mo>
      <m:mn>3,00</m:mn>
      <m:mspace width="0.2em"/>
