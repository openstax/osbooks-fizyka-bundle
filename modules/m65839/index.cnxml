                <m:mo>(</m:mo>
                <m:mrow>
                  <m:mi>k</m:mi>
                  <m:mi>x</m:mi>
                  <m:mo>−</m:mo>
                  <m:mi>ω</m:mi>
                  <m:mi>t</m:mi>
                  <m:mo>+</m:mo>
                  <m:mi>ϕ</m:mi>
                </m:mrow>
                <m:mo>)</m:mo>
              </m:mrow>
              <m:mo>=</m:mo>
              <m:mtext>sin</m:mtext>
              <m:mrow>
                <m:mo>(</m:mo>
                <m:mrow>
                  <m:mi>k</m:mi>
                  <m:mi>x</m:mi>
                  <m:mo>+</m:mo>
                  <m:mfrac>
                    <m:mi>ϕ</m:mi>
                    <m:mn>2</m:mn>
                  </m:mfrac>
                </m:mrow>
                <m:mo>)</m:mo>
              </m:mrow>
              <m:mtext>cos</m:mtext>
              <m:mrow>
                <m:mo>(</m:mo>
                <m:mrow>
                  <m:mi>ω</m:mi>
                  <m:mi>t</m:mi>
                  <m:mo>+</m:mo>
                  <m:mfrac>
                    <m:mi>ϕ</m:mi>
                    <m:mn>2</m:mn>
                  </m:mfrac>
                </m:mrow>
                <m:mo>)</m:mo>
              </m:mrow>
              <m:mo>+</m:mo>
              <m:mtext>cos</m:mtext>
              <m:mrow>
                <m:mo>(</m:mo>
                <m:mrow>
                  <m:mi>k</m:mi>
                  <m:mi>x</m:mi>
                  <m:mo>+</m:mo>
                  <m:mfrac>
                    <m:mi>ϕ</m:mi>
                    <m:mn>2</m:mn>
