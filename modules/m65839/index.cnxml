            <m:mrow>
              <m:mtext>m</m:mtext>
            </m:mrow>
            <m:mrow>
              <m:mn>−1</m:mn>
            </m:mrow>
          </m:msup>
          <m:mi>x</m:mi>
          <m:mo>−</m:mo>
          <m:mn>4</m:mn>
          <m:mspace width="0.2em"/>
          <m:msup>
            <m:mrow>
              <m:mtext>s</m:mtext>
