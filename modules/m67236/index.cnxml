        <row valign="top">
          <entry valign="middle" align="left">Napięcie prądu zmiennego</entry>
          <entry valign="middle" align="left">
            <m:math display="inline">
              <m:mrow>
                <m:mi>u</m:mi>
                <m:mo>⁡</m:mo>
                <m:mfenced>
                  <m:mi>t</m:mi>
                </m:mfenced>
              </m:mrow>
              <m:mo>=</m:mo>
              <m:mrow>
                <m:msub>
                  <m:mi>U</m:mi>
                  <m:mn>0</m:mn>
                </m:msub>
                <m:mo>⁢</m:mo>
                <m:mrow>
                  <m:mo>sin</m:mo>
                  <m:mfenced>
                    <m:mrow>
                      <m:mi>ω</m:mi>
