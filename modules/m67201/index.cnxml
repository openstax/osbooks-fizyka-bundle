            </m:mrow><m:mo>⋅</m:mo>
            <m:mover><m:mi>k</m:mi>
              <m:mo>̂</m:mo>
            </m:mover></m:mrow></m:mfenced></m:mrow></m:mtd></m:mtr><m:mtr><m:mtd><m:mphantom><m:mover><m:mi>F</m:mi>
        <m:mo>→</m:mo>
      </m:mover></m:mphantom><m:mo>=</m:mo>
      <m:mrow><m:mo>−</m:mo>
        <m:mrow><m:mrow><m:mrow><m:mrow><m:mn>2,4</m:mn>
                <m:mo>⋅</m:mo>
                <m:msup><m:mn>10</m:mn>
                  <m:mn>-14</m:mn>
                </m:msup></m:mrow><m:mo rspace="veryverythinmathspace" lspace="veryverythinmathspace">⁢</m:mo>
              <m:mi mathvariant="normal">N</m:mi>
