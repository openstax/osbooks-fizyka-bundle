                    <m:mi>r</m:mi>
                    <m:mn>2</m:mn>
                  </m:msub>
                  <m:mo>+</m:mo>
                  <m:mi>…</m:mi>
                  <m:mo>+</m:mo>
                  <m:msub>
                    <m:mi>r</m:mi>
                    <m:mrow>
                      <m:mi>N</m:mi>
                      <m:mo>−</m:mo>
                      <m:mn>1</m:mn>
                    </m:mrow>
                  </m:msub>
                  <m:mo>+</m:mo>
                  <m:msub>
                    <m:mi>r</m:mi>
                    <m:mi>N</m:mi>
                  </m:msub>
                </m:mrow>
              </m:mfenced>
            </m:mrow>
          </m:mrow>
