            <m:mi>U</m:mi>
            <m:mtext>bat</m:mtext>
          </m:msub>
          <m:mo>=</m:mo>
          <m:mrow>
            <m:mrow>
              <m:mrow>
                <m:msub>
                  <m:mi>ε</m:mi>
                  <m:mn>1</m:mn>
                </m:msub>
                <m:mo>−</m:mo>
                <m:mrow>
                  <m:mi>I</m:mi>
                  <m:mo>⁢</m:mo>
                  <m:msub>
                    <m:mi>r</m:mi>
                    <m:mn>1</m:mn>
                  </m:msub>
