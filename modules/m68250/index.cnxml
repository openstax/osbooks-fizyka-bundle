                                <m:mn>0</m:mn>
                                <m:mi>L</m:mi>
                              </m:munderover><m:mrow><m:msqrt><m:mfrac><m:mn>2</m:mn>
                                    <m:mi>L</m:mi>
                                  </m:mfrac></m:msqrt><m:mo>⁢</m:mo>
                                <m:mrow><m:mo>sin</m:mo>
                                  <m:mrow><m:mfenced><m:mfrac><m:mrow><m:mi>n</m:mi>
                                          <m:mo>⁢</m:mo>
                                          <m:mi>π</m:mi>
                                          <m:mo>⁢</m:mo>
                                          <m:mi>x</m:mi>
                                        </m:mrow><m:mi>L</m:mi>
