                              <m:mi>x</m:mi>
                            </m:mrow></m:mrow></m:mrow></m:mtd></m:mtr><m:mtr><m:mtd>
                        <m:mtext/>
                        </m:mtd><m:mtd><m:mo form="infix">=</m:mo>
                        <m:mrow><m:mo>−</m:mo>
                          <m:mrow><m:mi>i</m:mi>
                            <m:mo>⁢</m:mo>
