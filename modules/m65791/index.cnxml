                      </m:mrow>
                    </m:mrow>
                    <m:mo>)</m:mo>
                  </m:mrow>
                  <m:mn>2</m:mn>
                </m:msup>
                <m:mrow>
                  <m:mtext>/</m:mtext>
                </m:mrow>
                <m:mrow>
                  <m:mo>(</m:mo>
                  <m:mrow>
                    <m:mn>1</m:mn>
