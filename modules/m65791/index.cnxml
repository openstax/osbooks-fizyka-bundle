  <glossary>
    <definition id="ked-1bktdrs73_kda">
      <term cmlnle:reference="przyspieszenie kątowe" class="no-emphasis">przyspieszenie kątowe (ang. <foreign><term cxlxt:index="foreign" class="no-emphasis">angular acceleration</term></foreign>)</term>
      <meaning id="ked-1bktdrs73_kdb">szybkość zmian prędkości kątowej</meaning>
    </definition>
    <definition id="ked-1bktdrs73_kdc">
      <term cmlnle:reference="położenie kątowe" class="no-emphasis">położenie kątowe (ang. <foreign><term cxlxt:index="foreign" class="no-emphasis">angular position</term></foreign>)</term>
      <meaning id="ked-1bktdrs73_kdd">kąt pomiędzy wektorem położenia ciała a wybraną osią układu współrzędnych</meaning>
    </definition>
    <definition id="ked-1bktdrs73_kde">
      <term cmlnle:reference="chwilowe przyspieszenie kątowe" class="no-emphasis">chwilowe przyspieszenie kątowe (ang. <foreign><term cxlxt:index="foreign" class="no-emphasis">instantaneous angular acceleration</term></foreign>)</term>
      <meaning id="ked-1bktdrs73_kdf">pochodna prędkości kątowej po czasie</meaning>
    </definition>
    <definition id="ked-1bktdrs73_kdg">
      <term cmlnle:reference="chwilowa prędkość kątowa" class="no-emphasis">chwilowa prędkość kątowa (ang. <foreign><term cxlxt:index="foreign" class="no-emphasis">instantaneous angular velocity</term></foreign>)</term>
      <meaning id="ked-1bktdrs73_kdh">pochodna położenia kątowego po czasie</meaning>
    </definition>
  </glossary>
