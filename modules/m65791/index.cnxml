                    <m:mi mathvariant="normal">a</m:mi>
                    <m:mi mathvariant="normal">d</m:mi>
                  </m:mrow>
                </m:mrow>
                <m:mo>)</m:mo>
              </m:mrow>
              <m:mrow>
                <m:mtext>/</m:mtext>
              </m:mrow>
              <m:mrow>
                <m:mo>(</m:mo>
                <m:mrow>
                  <m:mn>45</m:mn>
                  <m:mn>,</m:mn>
                  <m:mn>0</m:mn>
                  <m:mspace width="0.2em"/>
                  <m:mrow>
                    <m:mi mathvariant="normal">s</m:mi>
                  </m:mrow>
                </m:mrow>
                <m:mo>)</m:mo>
              </m:mrow>
              <m:mo>=</m:mo>
              <m:mn>0</m:mn>
              <m:mn>,</m:mn>
              <m:mn>14</m:mn>
              <m:mspace width="0.2em"/>
              <m:mrow>
                <m:mi mathvariant="normal">r</m:mi>
                <m:mi mathvariant="normal">a</m:mi>
                <m:mi mathvariant="normal">d</m:mi>
                <m:mrow>
                  <m:mtext>/</m:mtext>
