            <m:math display="inline">
              <m:semantics>
                <m:mrow>
                  <m:mrow>
                    <m:mo>Δ</m:mo>
                    <m:mi>Q</m:mi>
                  </m:mrow>
                  <m:mo>=</m:mo>
                  <m:mrow>
                    <m:mi>n</m:mi>
                    <m:mo>⁢</m:mo>
                    <m:msub>
                      <m:mi>C</m:mi>
                      <m:mi>V</m:mi>
                    </m:msub>
                    <m:mo>⁢</m:mo>
                    <m:mrow>
                      <m:mo>Δ</m:mo>
                      <m:mi>T</m:mi>
                    </m:mrow>
                  </m:mrow>
                </m:mrow>
                <m:annotation encoding="application/vnd.katalysteducation.mathtex"> \prefop{\Delta} Q = nC_V \prefop{\Delta}T</m:annotation>
              </m:semantics>
            </m:math>
          </entry>
        </row>
        <row id="odt-table-row-51">
          <entry align="left" valign="middle">Ciepło molowe przy stałej objętości dla gazu doskonałego o <m:math>
            <m:mi>d</m:mi>
            </m:math> stopniach swobody</entry>
            <entry align="left" valign="middle">
              <m:math display="inline">
                <m:msub>
                  <m:mi>C</m:mi>
                  <m:mi>V</m:mi>
                </m:msub>
                <m:mo>=</m:mo>
                <m:mrow>
                  <m:mfrac>
                    <m:mi>d</m:mi>
                    <m:mn>2</m:mn>
                  </m:mfrac>
                  <m:mo>⁢</m:mo>
                  <m:mi>R</m:mi>
                </m:mrow>
              </m:math>
            </entry>
          </row>
          <row id="odt-table-row-52">
            <entry align="left" valign="middle">Rozkład Maxwella-Boltzmanna</entry>
            <entry align="left" valign="middle">
              <m:math display="inline">
                <m:mrow>
                  <m:mi>f</m:mi>
                  <m:mo>⁡</m:mo>
                  <m:mfenced>
                    <m:mi>v</m:mi>
                  </m:mfenced>
                </m:mrow>
                <m:mo>=</m:mo>
                <m:mrow>
                  <m:mrow>
                    <m:mrow>
                      <m:mfrac>
                        <m:mn>4</m:mn>
                        <m:msqrt>
                          <m:mi>π</m:mi>
                        </m:msqrt>
                      </m:mfrac>
                      <m:mo>⁢</m:mo>
                      <m:msup>
                        <m:mfenced>
                          <m:mfrac>
                            <m:mi>m</m:mi>
                            <m:mrow>
                              <m:mn>2</m:mn>
                              <m:mo>⁢</m:mo>
                              <m:msub>
                                <m:mi>k</m:mi>
                                <m:mtext>B</m:mtext>
                              </m:msub>
                              <m:mo>⁢</m:mo>
                              <m:mi>T</m:mi>
                            </m:mrow>
                          </m:mfrac>
                        </m:mfenced>
                        <m:mrow>
                          <m:mn>3</m:mn>
                          <m:mo>∕</m:mo>
                          <m:mn>2</m:mn>
                        </m:mrow>
                      </m:msup>
                    </m:mrow>
                    <m:mo>⋅</m:mo>
                    <m:msup>
                      <m:mi>v</m:mi>
                      <m:mn>2</m:mn>
                    </m:msup>
                  </m:mrow>
                  <m:mo>⁢</m:mo>
                  <m:msup>
                    <m:mi>e</m:mi>
                    <m:mrow>
                      <m:mo>−</m:mo>
                      <m:mrow>
                        <m:mrow>
                          <m:mi>m</m:mi>
                          <m:mo>⁢</m:mo>
                          <m:msup>
                            <m:mi>v</m:mi>
                            <m:mn>2</m:mn>
                          </m:msup>
                        </m:mrow>
                        <m:mo>∕</m:mo>
                        <m:mfenced>
                          <m:mrow>
                            <m:mn>2</m:mn>
                            <m:mo>⁢</m:mo>
                            <m:msub>
                              <m:mi>k</m:mi>
                              <m:mtext>B</m:mtext>
                            </m:msub>
                            <m:mo>⁢</m:mo>
                            <m:mi>T</m:mi>
                          </m:mrow>
                        </m:mfenced>
                      </m:mrow>
                    </m:mrow>
                  </m:msup>
                </m:mrow>
              </m:math>
            </entry>
          </row>
          <row id="odt-table-row-53">
            <entry align="left" valign="middle">Średnia prędkość cząsteczki gazu doskonałego</entry>
            <entry align="left" valign="middle">
              <m:math display="inline">
                <m:mover>
                  <m:mi>v</m:mi>
