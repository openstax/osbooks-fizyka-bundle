                <m:mo>⁢</m:mo>
                <m:mi>T</m:mi>
              </m:mrow>
            </m:math>
          </entry>
        </row>
        <row id="odt-table-row-43">
          <entry align="left" valign="middle">Równanie van der Waalsa</entry>
          <entry align="left" valign="middle">
            <m:math display="inline">
              <m:mrow>
                <m:mfenced open="[" close="]">
                  <m:mrow>
                    <m:mi>p</m:mi>
                    <m:mo>+</m:mo>
                    <m:mrow>
                      <m:mi>a</m:mi>
                      <m:mo>⁢</m:mo>
                      <m:msup>
                        <m:mfenced>
                          <m:mfrac>
                            <m:mi>n</m:mi>
                            <m:mi>V</m:mi>
                          </m:mfrac>
                        </m:mfenced>
                        <m:mn>2</m:mn>
                      </m:msup>
                    </m:mrow>
                  </m:mrow>
                </m:mfenced>
                <m:mo>⁢</m:mo>
                <m:mfenced>
                  <m:mrow>
                    <m:mi>V</m:mi>
                    <m:mo>−</m:mo>
                    <m:mrow>
                      <m:mi>n</m:mi>
                      <m:mo>⁢</m:mo>
                      <m:mi>b</m:mi>
                    </m:mrow>
                  </m:mrow>
                </m:mfenced>
              </m:mrow>
              <m:mo>=</m:mo>
              <m:mrow>
                <m:mi>n</m:mi>
                <m:mo>⁢</m:mo>
                <m:mi>R</m:mi>
                <m:mo>⁢</m:mo>
                <m:mi>T</m:mi>
              </m:mrow>
            </m:math>
          </entry>
        </row>
        <row id="odt-table-row-44">
          <entry align="left" valign="middle">Podstawowe równanie teorii kinetyczno-molekularnej</entry>
          <entry align="left" valign="middle">
            <m:math display="inline">
              <m:mrow>
                <m:mi>p</m:mi>
                <m:mo>⁢</m:mo>
                <m:mi>V</m:mi>
              </m:mrow>
              <m:mo>=</m:mo>
              <m:mrow>
                <m:mfrac>
                  <m:mn>1</m:mn>
                  <m:mn>3</m:mn>
                </m:mfrac>
                <m:mo>⁢</m:mo>
                <m:mi>N</m:mi>
                <m:mo>⁢</m:mo>
                <m:mi>m</m:mi>
                <m:mo>⁢</m:mo>
                <m:mover>
                  <m:msup>
                    <m:mi>v</m:mi>
                    <m:mn>2</m:mn>
                  </m:msup>
                  <m:mo>‾</m:mo>
                </m:mover>
              </m:mrow>
            </m:math>
          </entry>
        </row>
        <row id="odt-table-row-45">
          <entry align="left" valign="middle">Średnia prędkość kwadratowa</entry>
          <entry align="left" valign="middle">
            <m:math display="inline">
              <m:msub>
                <m:mi>v</m:mi>
                <m:mtext>k</m:mtext>
              </m:msub>
              <m:mo>=</m:mo>
              <m:msqrt>
                <m:mover>
                  <m:msup>
                    <m:mi>v</m:mi>
                    <m:mn>2</m:mn>
                  </m:msup>
                  <m:mo>‾</m:mo>
                </m:mover>
              </m:msqrt>
              <m:mo>=</m:mo>
              <m:msqrt>
                <m:mfrac>
                  <m:mrow>
                    <m:mn>3</m:mn>
                    <m:mo>⁢</m:mo>
                    <m:msub>
                      <m:mi>k</m:mi>
                      <m:mtext>B</m:mtext>
                    </m:msub>
                    <m:mo>⁢</m:mo>
                    <m:mi>T</m:mi>
                  </m:mrow>
                  <m:mi>m</m:mi>
                </m:mfrac>
              </m:msqrt>
              <m:mo>=</m:mo>
              <m:msqrt>
                <m:mfrac>
                  <m:mrow>
                    <m:mn>3</m:mn>
                    <m:mo>⁢</m:mo>
                    <m:mi>R</m:mi>
                    <m:mo>⁢</m:mo>
                    <m:mi>T</m:mi>
                  </m:mrow>
                  <m:mi>M</m:mi>
                </m:mfrac>
              </m:msqrt>
            </m:math>
          </entry>
        </row>
        <row id="odt-table-row-46">
          <entry align="left" valign="middle">Średnia droga swobodna</entry>
          <entry align="left" valign="middle">
            <m:math display="inline">
              <m:mi>λ</m:mi>
              <m:mo>=</m:mo>
              <m:mfrac>
                <m:mi>V</m:mi>
                <m:mrow>
                  <m:mn>4</m:mn>
                  <m:mo>⁢</m:mo>
                  <m:msqrt>
                    <m:mn>2</m:mn>
                  </m:msqrt>
                  <m:mo>⁢</m:mo>
                  <m:mi>π</m:mi>
                  <m:mo>⁢</m:mo>
                  <m:msup>
                    <m:mi>r</m:mi>
                    <m:mn>2</m:mn>
                  </m:msup>
                  <m:mo>⁢</m:mo>
                  <m:mi>N</m:mi>
                </m:mrow>
              </m:mfrac>
              <m:mo>=</m:mo>
              <m:mfrac>
                <m:mrow>
                  <m:msub>
                    <m:mi>k</m:mi>
                    <m:mtext>B</m:mtext>
                  </m:msub>
                  <m:mo>⁢</m:mo>
                  <m:mi>T</m:mi>
                </m:mrow>
                <m:mrow>
                  <m:mn>4</m:mn>
                  <m:mo>⁢</m:mo>
                  <m:msqrt>
                    <m:mn>2</m:mn>
                  </m:msqrt>
                  <m:mo>⁢</m:mo>
                  <m:mi>π</m:mi>
                  <m:mo>⁢</m:mo>
                  <m:msup>
                    <m:mi>r</m:mi>
                    <m:mn>2</m:mn>
                  </m:msup>
                  <m:mo>⁢</m:mo>
                  <m:mi>p</m:mi>
                </m:mrow>
              </m:mfrac>
              <m:mo>=</m:mo>
              <m:mfrac>
                <m:mrow>
                  <m:msub>
                    <m:mi>k</m:mi>
                    <m:mtext>B</m:mtext>
                  </m:msub>
                  <m:mo>⁢</m:mo>
                  <m:mi>T</m:mi>
                </m:mrow>
                <m:mrow>
                  <m:msqrt>
                    <m:mn>2</m:mn>
                  </m:msqrt>
                  <m:mo>⁢</m:mo>
                  <m:mi>σ</m:mi>
                  <m:mo>⁢</m:mo>
                  <m:mi>p</m:mi>
                </m:mrow>
              </m:mfrac>
            </m:math>
          </entry>
        </row>
        <row id="odt-table-row-47">
          <entry align="left" valign="middle">Średni czas między zderzeniami</entry>
          <entry align="left" valign="middle">
            <m:math display="inline">
              <m:mi>τ</m:mi>
              <m:mo>=</m:mo>
              <m:mfrac>
                <m:mrow>
                  <m:msub>
                    <m:mi>k</m:mi>
                    <m:mtext>B</m:mtext>
                  </m:msub>
                  <m:mo>⁢</m:mo>
