    <m:mo rspace="verythinmathspace" lspace="0px">⁢</m:mo>
    <m:mi mathvariant="normal">kg</m:mi>
  </m:mrow>
  </m:math>. Podstawiamy dane i obliczamy wartości</para><equation id="imp-idm1019644272" class="unnumbered"><m:math display="block"><m:semantics><m:mtable columnalign="left" displaystyle="true"><m:mtr><m:mtd><m:mfrac><m:mrow><m:mo rspace="veryverythinmathspace" lspace="verythinmathspace">d</m:mo><m:msub><m:mi>N</m:mi><m:msub><m:mi>v</m:mi><m:mn>2</m:mn></m:msub></m:msub></m:mrow><m:mrow><m:mo rspace="veryverythinmathspace" lspace="verythinmathspace">d</m:mo><m:msub><m:mi>N</m:mi><m:msub><m:mi>v</m:mi><m:mn>1</m:mn></m:msub></m:msub></m:mrow></m:mfrac><m:mo id="U7f8370fb-d0fa-4c16-8926-78de743cbbcb">=</m:mo><m:mfrac><m:mrow><m:mi>f</m:mi><m:mo>⁡</m:mo><m:mfenced><m:msub><m:mi>v</m:mi><m:mn>2</m:mn></m:msub></m:mfenced></m:mrow><m:mrow><m:mi>f</m:mi><m:mo>⁡</m:mo><m:mfenced><m:msub><m:mi>v</m:mi><m:mn>1</m:mn></m:msub></m:mfenced></m:mrow></m:mfrac></m:mtd></m:mtr><m:mtr><m:mtd><m:mphantom><m:mfrac><m:mrow><m:mo rspace="veryverythinmathspace" lspace="verythinmathspace">d</m:mo><m:msub><m:mi>N</m:mi><m:msub><m:mi>v</m:mi><m:mn>2</m:mn></m:msub></m:msub></m:mrow><m:mrow><m:mo rspace="veryverythinmathspace" lspace="verythinmathspace">d</m:mo><m:msub><m:mi>N</m:mi><m:msub><m:mi>v</m:mi><m:mn>1</m:mn></m:msub></m:msub></m:mrow></m:mfrac></m:mphantom><m:mo>=</m:mo><m:mrow><m:mfenced close="]" open="["><m:mrow><m:mrow><m:mfrac><m:mn>4</m:mn><m:msqrt><m:mi>π</m:mi></m:msqrt></m:mfrac><m:mo>⋅</m:mo><m:msup><m:mfenced><m:mfrac><m:mi>m</m:mi><m:mrow><m:mn>2</m:mn><m:mo>⁢</m:mo><m:msub><m:mi>k</m:mi><m:mtext>B</m:mtext></m:msub><m:mo>⁢</m:mo><m:mi>T</m:mi></m:mrow></m:mfrac></m:mfenced><m:mrow><m:mn>3</m:mn><m:mo>∕</m:mo><m:mn>2</m:mn></m:mrow></m:msup></m:mrow><m:mo>⁢</m:mo><m:msubsup><m:mi>v</m:mi><m:mn>2</m:mn><m:mn>2</m:mn></m:msubsup><m:mo>⁢</m:mo><m:msup><m:mi>e</m:mi><m:mrow><m:mo>−</m:mo><m:mrow><m:mrow><m:mi>m</m:mi><m:mo>⁢</m:mo><m:msubsup><m:mi>v</m:mi><m:mn>2</m:mn><m:mn>2</m:mn></m:msubsup></m:mrow><m:mo>∕</m:mo><m:mfenced><m:mrow><m:mn>2</m:mn><m:mo>⁢</m:mo><m:msub