<glossary>
  <definition id="fs-id1170902155009">
    <term xmlns:cmlnle="http://katalysteducation.org/cmlnle/1.0" class="no-emphasis" cmlnle:reference="moc elektryczna">moc elektryczna (ang. <foreign><term xmlns:cxlxt="http://katalysteducation.org/cxlxt/1.0" cxlxt:index="foreign" class="no-emphasis">electric power</term></foreign>)</term>
      <meaning id="fs-id1170902350158">prędkość zmian energii elektrycznej w czasie w obwodzie elektrycznym</meaning>
    </definition>
  </glossary>
