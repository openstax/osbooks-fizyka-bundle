        </m:msub>
        <m:mo>⁢</m:mo>
        <m:mfenced>
          <m:mi>t</m:mi>
        </m:mfenced>
      </m:mrow>
    </m:mfenced>
    <m:mo>=</m:mo>
    <m:mrow>
      <m:mi>ε</m:mi>
      <m:mo>⁢</m:mo>
      <m:msup>
        <m:mi>e</m:mi>
        <m:mrow>
          <m:mo>−</m:mo>
          <m:mrow>
            <m:mi>t</m:mi>
            <m:mo>∕</m:mo>
            <m:msub>
              <m:mi>τ</m:mi>
