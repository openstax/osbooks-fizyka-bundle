                      <m:mo>⁢</m:mo>
                      <m:mfenced>
                        <m:mrow>
                          <m:mi>E</m:mi>
                          <m:mo>&lt;</m:mo>
                          <m:msub>
                            <m:mi>E</m:mi>
                            <m:mtext>F</m:mtext>
                          </m:msub>
                        </m:mrow>
                      </m:mfenced>
                      <m:mtext>,</m:mtext>
                    </m:mrow>
                  </m:mtd>
                </m:mtr>
                <m:mtr>
                  <m:mtd>
                    <m:mrow>
                      <m:mi>f</m:mi>
                      <m:mo>⁡</m:mo>
                      <m:mfenced>
                        <m:mi>E</m:mi>
                      </m:mfenced>
                      <m:maligngroup/>
                    </m:mrow>
                    <m:mo>=</m:mo>
                    <m:mrow>
                      <m:mn>0</m:mn>
                      <m:mo>⁢</m:mo>
                      <m:mtext> </m:mtext>
                      <m:mo>⁢</m:mo>
                      <m:mfenced>
                        <m:mrow>
                          <m:mi>E</m:mi>
                          <m:mo>&gt;</m:mo>
                          <m:msub>
                            <m:mi>E</m:mi>
                            <m:mtext>F</m:mtext>
                          </m:msub>
