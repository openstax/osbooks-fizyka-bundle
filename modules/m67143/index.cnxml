      </m:msub>
      <m:mo id="Ucbfb0aa2-b169-4528-b789-0bbed1c0e72c-copy">=</m:mo>
      <m:mrow>
        <m:mfenced>
          <m:mrow>
            <m:msub>
              <m:mi>β</m:mi>
