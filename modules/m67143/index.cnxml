    <m:annotation encoding="application/vnd.katalysteducation.mathtex"> \begin{multiline} V_{\text{wylana}} &amp;= (\beta_{\text{wylana}} - \beta_{\text{S}}) V \prefop{\Delta} T \\ &amp;= (\SI[per-mode=reciprocal]{950e-6}{\per\celsius} - \SI[per-mode=reciprocal]{35e-6}{\per\celsius}) \cdot 601 \cdot \SI{20}{\celsius} = \SI{1,11}{\litre} \text{.} \end{multiline}</m:annotation>
    <m:annotation-xml encoding="application/vnd.katalysteducation.legtool.mathml.original+xml">
