              </m:munder>
              <m:mrow>
                <m:msub>
                  <m:mfenced>
                    <m:mfrac>
                      <m:mrow>
                        <m:mi>λ</m:mi>
                        <m:mo>⁢</m:mo>
                        <m:mrow>
                          <m:mo lspace="verythinmathspace" rspace="veryverythinmathspace">d</m:mo>
                          <m:mi>l</m:mi>
                        </m:mrow>
                      </m:mrow>
                      <m:msup>
                        <m:mi>r</m:mi>
                        <m:mn>2</m:mn>
                      </m:msup>
                    </m:mfrac>
                  </m:mfenced>
                  <m:mi>x</m:mi>
                </m:msub>
                <m:mo>⁢</m:mo>
                <m:mtext>,</m:mtext>
              </m:mrow>
            </m:mrow>
          </m:mrow>
        </m:mtd>
      </m:mtr>
      <m:mtr>
        <m:mtd>
          <m:mrow>
            <m:msub>
              <m:mi>E</m:mi>
              <m:mi>y</m:mi>
            </m:msub>
            <m:mo>⁢</m:mo>
            <m:mfenced>
              <m:mi>P</m:mi>
            </m:mfenced>
            <m:maligngroup/>
          </m:mrow>
          <m:mo>=</m:mo>
          <m:mrow>
            <m:mfrac>
              <m:mn>1</m:mn>
              <m:mrow>
                <m:mn>4</m:mn>
                <m:mo>⁢</m:mo>
                <m:mi>π</m:mi>
                <m:mo>⁢</m:mo>
                <m:msub>
                  <m:mi>ε</m:mi>
                  <m:mn>0</m:mn>
                </m:msub>
              </m:mrow>
            </m:mfrac>
            <m:mo>⁢</m:mo>
            <m:mrow>
              <m:munder>
                <m:mo>∫</m:mo>
                <m:mtext>po linii</m:mtext>
              </m:munder>
              <m:mrow>
                <m:msub>
                  <m:mfenced>
                    <m:mfrac>
                      <m:mrow>
                        <m:mi>λ</m:mi>
                        <m:mo>⁢</m:mo>
                        <m:mrow>
                          <m:mo lspace="verythinmathspace" rspace="veryverythinmathspace">d</m:mo>
                          <m:mi>l</m:mi>
                        </m:mrow>
                      </m:mrow>
                      <m:msup>
                        <m:mi>r</m:mi>
                        <m:mn>2</m:mn>
                      </m:msup>
                    </m:mfrac>
                  </m:mfenced>
                  <m:mi>y</m:mi>
                </m:msub>
                <m:mo>⁢</m:mo>
                <m:mtext>,</m:mtext>
              </m:mrow>
            </m:mrow>
          </m:mrow>
        </m:mtd>
      </m:mtr>
      <m:mtr>
        <m:mtd>
          <m:mrow>
            <m:msub>
              <m:mi>E</m:mi>
              <m:mi>z</m:mi>
            </m:msub>
            <m:mo>⁢</m:mo>
            <m:mfenced>
              <m:mi>P</m:mi>
            </m:mfenced>
            <m:maligngroup/>
          </m:mrow>
          <m:mo>=</m:mo>
          <m:mrow>
            <m:mfrac>
              <m:mn>1</m:mn>
              <m:mrow>
                <m:mn>4</m:mn>
                <m:mo>⁢</m:mo>
                <m:mi>π</m:mi>
                <m:mo>⁢</m:mo>
                <m:msub>
                  <m:mi>ε</m:mi>
                  <m:mn>0</m:mn>
                </m:msub>
              </m:mrow>
            </m:mfrac>
            <m:mo>⁢</m:mo>
            <m:mrow>
              <m:munder>
                <m:mo>∫</m:mo>
                <m:mtext>po linii</m:mtext>
              </m:munder>
              <m:mrow>
                <m:msub>
                  <m:mfenced>
                    <m:mfrac>
                      <m:mrow>
                        <m:mi>λ</m:mi>
                        <m:mo>⁢</m:mo>
                        <m:mrow>
                          <m:mo lspace="verythinmathspace" rspace="veryverythinmathspace">d</m:mo>
                          <m:mi>l</m:mi>
                        </m:mrow>
                      </m:mrow>
                      <m:msup>
                        <m:mi>r</m:mi>
                        <m:mn>2</m:mn>
                      </m:msup>
                    </m:mfrac>
                  </m:mfenced>
