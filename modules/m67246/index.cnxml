<glossary>
  <definition id="fs-idm349140640">
    <term cmlnle:reference="pierwsza zasada termodynamiki" class="no-emphasis"> pierwsza zasada termodynamiki (ang. <foreign><term cxlxt:index="foreign" class="no-emphasis">first law of thermodynamics</term></foreign>)</term>
      <meaning id="fs-idm347041568">zmiana energii wewnętrznej dla dowolnego przejścia między dwoma stanami równowagi jest równa <m:math display="inline">
        <m:mrow>
          <m:mtext>Δ</m:mtext>
          <m:mo>⁢</m:mo>
          <m:mi>U</m:mi>
        </m:mrow>
        <m:mo>=</m:mo>
        <m:mrow>
          <m:mi>Q</m:mi>
          <m:mo>−</m:mo>
          <m:mi>W</m:mi>
        </m:mrow>
      </m:math>
    </meaning>
  </definition>
</glossary>
