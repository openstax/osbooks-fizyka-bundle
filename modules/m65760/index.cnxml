          <m:msub>
            <m:mi>F</m:mi>
            <m:mn>1</m:mn>
          </m:msub>
          <m:mo>=</m:mo>
          <m:mn>1</m:mn>
          <m:mspace width="0.2em"/>
          <m:mn>350</m:mn>
          <m:mspace width="0.2em"/>
          <m:mtext>N</m:mtext>
          <m:mn>,</m:mn>
          <m:mspace width="1em"/>
          <m:msub>
            <m:mi>F</m:mi>
            <m:mn>2</m:mn>
          </m:msub>
          <m:mo>=</m:mo>
          <m:mn>1</m:mn>
          <m:mspace width="0.2em"/>
          <m:mn>365</m:mn>
          <m:mspace width="0.2em"/>
          <m:mtext>N</m:mtext>
          <m:mspace linebreak="newline"/>
          <m:mn>9</m:mn>
          <m:mrow>
            <m:mo>(</m:mo>
            <m:mrow>
              <m:msub>
                <m:mi>F</m:mi>
                <m:mn>2</m:mn>
              </m:msub>
              <m:mo>−</m:mo>
              <m:msub>
                <m:mi>F</m:mi>
                <m:mn>1</m:mn>
