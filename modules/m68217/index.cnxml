      </m:math>
    </entry>
    <entry valign="top" align="left">
      <m:math>
        <m:mrow>
          <m:mtable>
            <m:mtr>
              <m:mtd columnalign="left">
                <m:mo>+</m:mo>
                <m:mspace width="0.2em"/>
                <m:mtext>dla obrazu rzeczywistego</m:mtext>
              </m:mtd>
            </m:mtr>
            <m:mtr>
              <m:mtd columnalign="left">
                <m:mo>−</m:mo>
                <m:mspace width="0.2em"/>
                <m:mtext>dla obrazu pozornego</m:mtext>
              </m:mtd>
            </m:mtr>
          </m:mtable>
        </m:mrow>
      </m:math>
    </entry>
  </row>
  <row valign="top">
    <entry valign="top" align="left">Powiększenie <m:math display="inline">
      <m:mi>p</m:mi>
    </m:math>
  </entry>
  <entry valign="top" align="left">
    <m:math>
      <m:mrow>
        <m:mtable>
          <m:mtr>
            <m:mtd columnalign="left">
              <m:mo>+</m:mo>
              <m:mspace width="0.2em"/>
              <m:mtext>dla obrazu prostego</m:mtext>
            </m:mtd>
          </m:mtr>
          <m:mtr>
            <m:mtd columnalign="left">
              <m:mo>−</m:mo>
              <m:mspace width="0.2em"/>
              <m:mtext>dla obrazu odwróconego</m:mtext>
            </m:mtd>
          </m:mtr>
        </m:mtable>
      </m:mrow>
    </m:math>
  </entry>
</row>
<row valign="top">
  <entry valign="top" align="left">Równanie na głębokość pozorną</entry>
  <entry valign="top" align="left">
    <m:math display="inline">
      <m:msub>
        <m:mi>h</m:mi>
        <m:mtext>o</m:mtext>
      </m:msub>
      <m:mo>=</m:mo>
      <m:mrow>
        <m:mfrac>
          <m:msub>
            <m:mi>n</m:mi>
            <m:mn>2</m:mn>
          </m:msub>
          <m:msub>
            <m:mi>n</m:mi>
            <m:mn>1</m:mn>
          </m:msub>
        </m:mfrac>
        <m:mo>⁢</m:mo>
        <m:msub>
          <m:mi>h</m:mi>
          <m:mtext>p</m:mtext>
        </m:msub>
      </m:mrow>
    </m:math>
  </entry>
</row>
<row valign="top">
  <entry valign="top" align="left">Równanie powierzchni sferycznej</entry>
  <entry valign="top" align="left">
    <m:math display="inline">
      <m:mrow>
        <m:mfrac>
          <m:msub>
            <m:mi>n</m:mi>
            <m:mn>1</m:mn>
          </m:msub>
          <m:msub>
            <m:mi>d</m:mi>
            <m:mtext>p</m:mtext>
          </m:msub>
        </m:mfrac>
        <m:mo>+</m:mo>
        <m:mfrac>
          <m:msub>
            <m:mi>n</m:mi>
            <m:mn>2</m:mn>
          </m:msub>
          <m:msub>
            <m:mi>d</m:mi>
            <m:mtext>o</m:mtext>
          </m:msub>
        </m:mfrac>
      </m:mrow>
      <m:mo>=</m:mo>
      <m:mfrac>
        <m:mrow>
          <m:msub>
            <m:mi>n</m:mi>
            <m:mn>2</m:mn>
          </m:msub>
          <m:mo>−</m:mo>
          <m:msub>
            <m:mi>n</m:mi>
            <m:mn>1</m:mn>
          </m:msub>
        </m:mrow>
        <m:mi>R</m:mi>
      </m:mfrac>
    </m:math>
  </entry>
</row>
<row valign="top">
  <entry valign="top" align="left">Równanie cienkiej soczewki</entry>
  <entry valign="top" align="left">
    <m:math display="inline">
      <m:mrow>
        <m:mfrac>
          <m:mn>1</m:mn>
          <m:msub>
            <m:mi>d</m:mi>
            <m:mtext>p</m:mtext>
          </m:msub>
        </m:mfrac>
        <m:mo>+</m:mo>
        <m:mfrac>
          <m:mn>1</m:mn>
          <m:msub>
            <m:mi>d</m:mi>
            <m:mtext>o</m:mtext>
          </m:msub>
        </m:mfrac>
      </m:mrow>
      <m:mo>=</m:mo>
      <m:mfrac>
        <m:mn>1</m:mn>
        <m:mi>f</m:mi>
      </m:mfrac>
    </m:math>
  </entry>
</row>
<row valign="top">
  <entry valign="top" align="left">Równanie soczewki</entry>
  <entry valign="top" align="left">
    <m:math display="inline">
      <m:mfrac>
        <m:mn>1</m:mn>
        <m:mi>f</m:mi>
      </m:mfrac>
      <m:mo>=</m:mo>
      <m:mrow>
        <m:mfenced>
          <m:mrow>
            <m:mfrac>
              <m:msub>
                <m:mi>n</m:mi>
                <m:mn>2</m:mn>
              </m:msub>
              <m:msub>
                <m:mi>n</m:mi>
                <m:mn>1</m:mn>
              </m:msub>
            </m:mfrac>
            <m:mo>−</m:mo>
            <m:mn>1</m:mn>
          </m:mrow>
        </m:mfenced>
        <m:mo>⁢</m:mo>
        <m:mfenced>
          <m:mrow>
            <m:mfrac>
              <m:mn>1</m:mn>
              <m:msub>
                <m:mi>R</m:mi>
                <m:mn>1</m:mn>
              </m:msub>
            </m:mfrac>
            <m:mo>−</m:mo>
            <m:mfrac>
              <m:mn>1</m:mn>
              <m:msub>
                <m:mi>R</m:mi>
                <m:mn>2</m:mn>
              </m:msub>
            </m:mfrac>
          </m:mrow>
        </m:mfenced>
      </m:mrow>
    </m:math>
  </entry>
</row>
<row valign="top">
  <entry valign="top" align="left">Powiększenie przedmiotu</entry>
  <entry valign="top" align="left">
    <m:math display="inline">
      <m:mrow>
        <m:mi>p</m:mi>
        <m:mo>≡</m:mo>
        <m:mfrac>
          <m:msub>
            <m:mi>h</m:mi>
            <m:mtext>o</m:mtext>
          </m:msub>
          <m:msub>
            <m:mi>h</m:mi>
            <m:mtext>p</m:mtext>
          </m:msub>
        </m:mfrac>
      </m:mrow>
      <m:mo>=</m:mo>
      <m:mrow>
        <m:mo>−</m:mo>
        <m:mfrac>
          <m:msub>
            <m:mi>d</m:mi>
            <m:mtext>o</m:mtext>
          </m:msub>
          <m:msub>
            <m:mi>d</m:mi>
            <m:mtext>p</m:mtext>
          </m:msub>
        </m:mfrac>
      </m:mrow>
    </m:math>
  </entry>
</row>
<row valign="top">
  <entry valign="top" align="left">Zdolność skupiająca</entry>
  <entry valign="top" align="left">
    <m:math display="inline">
      <m:mi>Z</m:mi>
      <m:mo>=</m:mo>
      <m:mfrac>
        <m:mn>1</m:mn>
        <m:mi>f</m:mi>
      </m:mfrac>
    </m:math>
  </entry>
</row>
<row valign="top">
  <entry valign="top" align="left">Zdolność skupiająca kilku soczewek<newline/>znajdujących się blisko siebie</entry>
  <entry valign="top" align="left">
    <m:math display="inline">
      <m:msub>
        <m:mi>Z</m:mi>
        <m:mtext>całkowita</m:mtext>
      </m:msub>
      <m:mo>=</m:mo>
      <m:mrow>
        <m:msub>
          <m:mi>Z</m:mi>
          <m:mtext>soczewki 1</m:mtext>
        </m:msub>
        <m:mo>+</m:mo>
        <m:msub>
          <m:mi>Z</m:mi>
          <m:mtext>soczewki 2</m:mtext>
        </m:msub>
        <m:mo>+</m:mo>
        <m:msub>
          <m:mi>Z</m:mi>
          <m:mtext>soczewki 3</m:mtext>
        </m:msub>
        <m:mo>+</m:mo>
        <m:mi>…</m:mi>
      </m:mrow>
    </m:math>
  </entry>
</row>
<row valign="top">
  <entry valign="top" align="left">Powiększenie kątowe <m:math display="inline">
    <m:msub>
      <m:mi>p</m:mi>
      <m:mtext>k</m:mtext>
    </m:msub>
    </m:math> lupy</entry>
    <entry valign="top" align="left">
      <m:math display="inline">
        <m:msub>
          <m:mi>p</m:mi>
          <m:mtext>k</m:mtext>
        </m:msub>
        <m:mo>=</m:mo>
        <m:mfrac>
          <m:msub>
            <m:mi>θ</m:mi>
            <m:mtext>o</m:mtext>
          </m:msub>
          <m:msub>
            <m:mi>θ</m:mi>
            <m:mtext>p</m:mtext>
          </m:msub>
        </m:mfrac>
      </m:math>
    </entry>
  </row>
  <row valign="top">
    <entry valign="top" align="left">Powiększenie kątowe przedmiotu znajdującego się w odległości <m:math display="inline">
      <m:mi>L</m:mi>
    </m:math><newline/> od oka dla soczewki wypukłej o ogniskowej <m:math display="inline">
    <m:mi>f</m:mi>
  </m:math>, znajdującej się<newline/> w odległości <m:math display="inline">
  <m:mi>l</m:mi>
  </m:math> od oka</entry>
  <entry valign="top" align="left">
    <m:math display="inline">
      <m:msub>
        <m:mi>p</m:mi>
        <m:mtext>k</m:mtext>
      </m:msub>
      <m:mo>=</m:mo>
      <m:mrow>
        <m:mfrac>
          <m:mrow>
            <m:mn>25</m:mn>
            <m:mo>⁢</m:mo>
            <m:mi mathvariant="normal">cm</m:mi>
          </m:mrow>
          <m:mi>L</m:mi>
        </m:mfrac>
        <m:mo>⋅</m:mo>
        <m:mfenced>
          <m:mrow>
            <m:mn>1</m:mn>
            <m:mo>+</m:mo>
            <m:mfrac>
              <m:mrow>
                <m:mi>L</m:mi>
                <m:mo>−</m:mo>
                <m:mi>l</m:mi>
              </m:mrow>
              <m:mi>f</m:mi>
            </m:mfrac>
          </m:mrow>
        </m:mfenced>
      </m:mrow>
    </m:math>
  </entry>
</row>
<row valign="top">
  <entry valign="top" align="left">Zakres powiększenia kątowego dla danej soczewki dla osoby<newline/> o punkcie bliży równym <m:math display="inline">
    <m:mn>25</m:mn>
    <m:mo>⁢</m:mo>
    <m:mi mathvariant="normal">cm</m:mi>
  </m:math>
</entry>
<entry valign="top" align="left">
  <m:math display="inline">
    <m:mfrac>
      <m:mrow>
        <m:mn>25</m:mn>
        <m:mo>⁢</m:mo>
        <m:mi mathvariant="normal">cm</m:mi>
      </m:mrow>
      <m:mi>f</m:mi>
    </m:mfrac>
    <m:mo>≤</m:mo>
    <m:msub>
      <m:mi>p</m:mi>
      <m:mtext>k</m:mtext>
    </m:msub>
    <m:mo>≤</m:mo>
    <m:mrow>
      <m:mn>1</m:mn>
      <m:mo>+</m:mo>
      <m:mfrac>
        <m:mrow>
          <m:mn>25</m:mn>
          <m:mo>⁢</m:mo>
          <m:mi mathvariant="normal">cm</m:mi>
        </m:mrow>
        <m:mi>f</m:mi>
      </m:mfrac>
    </m:mrow>
  </m:math>
</entry>
</row>
<row valign="top">
  <entry valign="top" align="left">Powiększenie całkowite mikroskopu optycznego</entry>
  <entry valign="top" align="left">
    <m:math display="inline">
      <m:msub>
        <m:mi>p</m:mi>
        <m:mtext>cał</m:mtext>
      </m:msub>
      <m:mo>=</m:mo>
      <m:mrow>
        <m:msup>
          <m:mi>p</m:mi>
          <m:mtext>ob</m:mtext>
        </m:msup>
        <m:mo>⁢</m:mo>
        <m:msubsup>
          <m:mi>p</m:mi>
          <m:mtext>k</m:mtext>
          <m:mtext>ok</m:mtext>
        </m:msubsup>
      </m:mrow>
      <m:mo>=</m:mo>
      <m:mfrac>
        <m:mrow>
          <m:mo>−</m:mo>
          <m:mrow>
            <m:msubsup>
              <m:mi>d</m:mi>
              <m:mtext>o</m:mtext>
              <m:mtext>ob</m:mtext>
            </m:msubsup>
            <m:mo>⁢</m:mo>
            <m:mfenced>
              <m:mrow>
                <m:msup>
                  <m:mi>f</m:mi>
                  <m:mtext>ok</m:mtext>
                </m:msup>
                <m:mo>+</m:mo>
                <m:mrow>
                  <m:mn>25</m:mn>
                  <m:mo>⁢</m:mo>
                  <m:mi mathvariant="normal">cm</m:mi>
                </m:mrow>
              </m:mrow>
            </m:mfenced>
          </m:mrow>
        </m:mrow>
        <m:mrow>
          <m:msup>
            <m:mi>f</m:mi>
            <m:mtext>ob</m:mtext>
          </m:msup>
          <m:mo>⁢</m:mo>
          <m:msup>
            <m:mi>f</m:mi>
            <m:mtext>ok</m:mtext>
          </m:msup>
        </m:mrow>
      </m:mfrac>
    </m:math>
  </entry>
</row>
</tbody>





