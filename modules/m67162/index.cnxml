    <m:annotation encoding="application/vnd.katalysteducation.mathtex"> \begin{multiline} F &amp;= \frac{1}{4 \pi \epsilon_0} \cdot \frac{\abs{e}^2}{r^2} = \frac{1}{4 \pi \cdot \SI{8,85e-12}{\squared\coulomb\per\newton\per\squared\metre}} \cdot \frac{(\SI{1,602e-19}{\coulomb})^2}{(\SI{5,29e-11}{\metre})^2} \\ &amp;= \SI{8,25e-8}{\newton} \text{.} \end{multiline}</m:annotation>
