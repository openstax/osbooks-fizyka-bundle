    <row valign="top">
      <entry valign="top" align="left">
        <m:math display="inline">
          <m:mn>5</m:mn>
        </m:math>
      </entry>
      <entry valign="top" align="left">
        <m:math display="inline">
          <m:msqrt>
            <m:mn>30</m:mn>
          </m:msqrt>
          <m:mo>⁢</m:mo>
