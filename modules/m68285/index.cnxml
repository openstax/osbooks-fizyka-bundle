        </m:math>
      </entry>
      <entry valign="top" align="left">
        <m:math display="inline">
          <m:msqrt>
            <m:mn>6</m:mn>
          </m:msqrt>
          <m:mo>⁢</m:mo>
          <m:mi>h</m:mi>
        </m:math>
      </entry>
