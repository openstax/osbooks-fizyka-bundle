        <m:mover>
          <m:mi>r</m:mi>
          <m:mtext>→</m:mtext>
        </m:mover>
      </m:mrow>
      <m:mo stretchy="false">(</m:mo>
      <m:mi>t</m:mi>
      <m:mo stretchy="false">)</m:mo>
      <m:mo>=</m:mo>
      <m:mi>A</m:mi>
      <m:mo stretchy="false">(</m:mo>
      <m:mrow>
        <m:mover>
