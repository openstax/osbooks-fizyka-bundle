<m:math display="inline"><m:semantics><m:mfenced close="" open=""><m:mrow><m:mrow><m:mi>D</m:mi><m:mo>⁢</m:mo><m:mrow><m:mo>sin</m:mo><m:mi>θ</m:mi></m:mrow></m:mrow><m:mo>=</m:mo><m:mrow><m:mi>m</m:mi><m:mo>⁢</m:mo><m:mi>λ</m:mi><m:mo>⁢</m:mo><m:mtext>, gdzie </m:mtext><m:mo>⁢</m:mo><m:mi>m</m:mi></m:mrow><m:mo>=</m:mo><m:mrow><m:mtext/><m:mo>±</m:mo><m:mn>1</m:mn></m:mrow></m:mrow><m:mrow><m:mtext/><m:mo>±</m:mo><m:mn>2</m:mn></m:mrow><m:mrow><m:mtext/><m:mo>±</m:mo><m:mn>3</m:mn></m:mrow><m:mi>…</m:mi></m:mfenced><m:annotation encoding="application/vnd.katalysteducation.mathtex">            D\sin\theta = m\lambda \text{, gdzie } m=\text{}\pm 1,\text{}\pm 2,\text{}\pm 3,\dots</m:annotation></m:semantics></m:math>
