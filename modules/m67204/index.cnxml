        <m:mfenced close="|" open="|"><m:mover><m:mi>M</m:mi>
            <m:mo>→</m:mo>
          </m:mover></m:mfenced><m:mo id="U5dbe4e66-330f-4383-8b91-04b78b06e1e3">=</m:mo>
        <m:mfenced close="|" open="|"><m:mrow><m:mover><m:mi>μ</m:mi>
