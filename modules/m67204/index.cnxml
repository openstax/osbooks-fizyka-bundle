            </m:mover><m:mo>×</m:mo>
            <m:mover><m:mi>B</m:mi>
              <m:mo>→</m:mo>
            </m:mover></m:mrow></m:mfenced><m:mo>=</m:mo>
        <m:mrow><m:mi>μ</m:mi>
