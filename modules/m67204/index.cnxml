        <m:mrow><m:mrow><m:mrow><m:mn>6,3</m:mn>
              <m:mo>⋅</m:mo>
              <m:msup><m:mn>10</m:mn>
                <m:mn>-7</m:mn>
              </m:msup></m:mrow><m:mo rspace="veryverythinmathspace" lspace="veryverythinmathspace">⁢</m:mo>
            <m:mrow><m:mi mathvariant="normal">N</m:mi>
              <m:mo rspace="veryverythinmathspace" lspace="veryverythinmathspace">⁢</m:mo>
              <m:mi mathvariant="normal">m</m:mi>
            </m:mrow></m:mrow><m:mo>⁢</m:mo>
          <m:mtext>,</m:mtext>
        </m:mrow></m:mtd></m:mtr></m:mtable><m:annotation encoding="application/vnd.katalysteducation.mathtex"> \begin{multiline} M &amp;= \abs{\vec{M}} = \abs{\vec{\mu} \times \vec{B}} = \mu B \sin \theta = \SI{2,5e-6}{\ampere\metre\squared} \cdot \SI{0,5}{\tesla} \cdot \sin \ang{30} \\ &amp;= \SI{6,3e-7}{\newton\metre} \text{,} \end{multiline}</m:annotation>
    <m:annotation-xml encoding="application/vnd.katalysteducation.legtool.mathml.original+xml"><m:mi>M</m:mi>
