            <m:mo>⁢</m:mo>
            <m:mfenced><m:mrow><m:mi>a</m:mi>
                <m:mo>−</m:mo>
                <m:mi>x</m:mi>
              </m:mrow></m:mfenced><m:mo>⁢</m:mo>
            <m:mrow><m:mo>sin</m:mo>
              <m:mrow><m:mrow><m:mi>θ</m:mi>
                  <m:mo>⋅</m:mo>
