      </para><equation id="imp-idm253030240" class="unnumbered"><m:math display="block"><m:semantics><m:semantics><m:mtable columnalign="right left" columnspacing="0px" displaystyle="true"><m:mtr><m:mtd><m:msub><m:mi>E</m:mi><m:mtext>k</m:mtext></m:msub></m:mtd><m:mtd><m:mo form="infix">=</m:mo><m:mrow><m:mrow><m:mfrac><m:mrow><m:mi>m</m:mi><m:mo>⁢</m:mo><m:msup><m:mi>u</m:mi><m:mn>2</m:mn></m:msup></m:mrow><m:msqrt><m:mn>1</m:mn><m:mo>−</m:mo><m:mrow><m:msup><m:mi>u</m:mi><m:mn>2</m:mn></m:msup><m:mo>∕</m:mo><m:msup><m:mi>c</m:mi><m:mn>2</m:mn></m:msup></m:mrow></m:msqrt></m:mfrac><m:msubsup><m:mo>|</m:mo><m:mn>0</m:mn><m:mi>u</m:mi></m:msubsup></m:mrow><m:mo>−</m:mo><m:mrow><m:mi>m</m:mi><m:mo>⁢</m:mo><m:mrow><m:mo>∫</m:mo><m:mrow><m:mrow><m:mfrac><m:mi>u</m:mi><m:msqrt><m:mn>1</m:mn><m:mo>−</m:mo><m:mrow><m:msup><m:mi>u</m:mi><m:mn>2</m:mn></m:msup><m:mo>∕</m:mo><m:msup><m:mi>c</m:mi><m:mn>2</m:mn></m:msup></m:mrow></m:msqrt></m:mfrac><m:mo>⋅</m:mo><m:mfrac><m:mrow><m:mo rspace="veryverythinmathspace" lspace="verythinmathspace">d</m:mo><m:mi>u</m:mi></m:mrow><m:mrow><m:mo rspace="veryverythinmathspace" lspace="verythinmathspace">d</m:mo><m:mi>t</m:mi></m:mrow></m:mfrac></m:mrow><m:mo>⁢</m:mo><m:mrow><m:mo rspace="veryverythinmathspace" lspace="verythinmathspace">d</m:mo><m:mi>t</m:mi></m:mrow></m:mrow></m:mrow></m:mrow></m:mrow></m:mtd></m:mtr><m:mtr><m:mtd><m:mtext/></m:mtd><m:mtd><m:mo form="infix">=</m:mo><m:mrow><m:mfrac><m:mrow><m:mi>m</m:mi><m:mo>⁢</m:mo><m:msup><m:mi>u</m:mi><m:mn>2</m:mn></m:msup></m:mrow><m:msqrt><m:mn>1</m:mn><m:mo>−</m:mo><m:mrow><m:msup><m:mi>u</m:mi><m:mn>2</m:mn></m:msup><m:mo>∕</m:mo><m:msup><m:mi>c</m:mi><m:mn>2</m:mn></m:msup></m:mrow></m:msqrt></m:mfrac><m:mo>−</m:mo><m:mrow><m:mi>m</m:mi><m:mo>⁢</m:mo><m:mrow><m:mo>∫</m:mo><m:mrow><m:mfrac><m:mi>u</m:mi><m:msqrt><m:mn>1</m:mn><m:mo>−</m:mo><m:mrow><m:msup><m:mi>u</m:mi><m:mn>2</m:mn></m:msup><m:mo>∕</m:mo><m:msup><m:mi>c</m:mi><m:mn>2</m:mn></m:msup></m:mrow></m:msqrt></m:mfrac