            <m:mi mathvariant="normal">s</m:mi>
          </m:mrow></m:mrow></m:mrow></m:mtd></m:mtr><m:mtr><m:mtd><m:mphantom><m:mi>ω</m:mi>
      </m:mphantom><m:mo>=</m:mo>
      <m:mrow><m:mrow><m:mn>1047,2</m:mn>
          <m:mo rspace="veryverythinmathspace" lspace="veryverythinmathspace">⁢</m:mo>
          <m:mrow><m:mi mathvariant="normal">rad</m:mi>
            <m:mo>∕</m:mo>
            <m:mi mathvariant="normal">s</m:mi>
          </m:mrow></m:mrow><m:mo>−</m:mo>
        <m:mrow><m:mn>1012,71</m:mn>
          <m:mo rspace="veryverythinmathspace" lspace="veryverythinmathspace">⁢</m:mo>
          <m:mrow><m:mi mathvariant="normal">rad</m:mi>
            <m:mo>∕</m:mo>
            <m:mi mathvariant="normal">s</m:mi>
          </m:mrow></m:mrow></m:mrow><m:mo>=</m:mo>
      <m:mrow><m:mrow><m:mn>35,1</m:mn>
          <m:mo rspace="veryverythinmathspace" lspace="veryverythinmathspace">⁢</m:mo>
          <m:mrow><m:mi mathvariant="normal">rad</m:mi>
            <m:mo>∕</m:mo>
            <m:mi mathvariant="normal">s</m:mi>
          </m:mrow></m:mrow><m:mtext>.</m:mtext>
      </m:mrow></m:mtd></m:mtr></m:mtable><m:annotation encoding="application/vnd.katalysteducation.mathtex">            \begin{multiline} \omega &amp;= \omega_0 + \epsilon t \\ &amp;= \num{1,0e4} \cdot \frac{2\pi \si{\radian}}{\SI{60,0}{\second}} + (-\SI{34,9}{\radian\per\second\squared}) \cdot \SI{29,0}{\second} \\ &amp;= \SI{1047,2}{\radian\per\second} - \SI{1012,71}{\radian\per\second} = \SI{35,1}{\radian\per\second} \text{.} \end{multiline}</m:annotation>
