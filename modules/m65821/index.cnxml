                          <m:mi>A</m:mi>
                          <m:mo>−</m:mo>
                          <m:msub>
                            <m:mi>p</m:mi>
                            <m:mn>1</m:mn>
                          </m:msub>
                          <m:mi>A</m:mi>
                          <m:mo>=</m:mo>
                          <m:mo stretchy="false">(</m:mo>
                          <m:msub>
                            <m:mi>p</m:mi>
                            <m:mn>2</m:mn>
                          </m:msub>
                          <m:mo>−</m:mo>
                          <m:msub>
                            <m:mi>p</m:mi>
                            <m:mn>1</m:mn>
                          </m:msub>
                          <m:mo stretchy="false">)</m:mo>
                          <m:mi>A</m:mi>
                          <m:mo>=</m:mo>
                          <m:mrow>
                            <m:mo>(</m:mo>
                            <m:mrow>
                              <m:msub>
                                <m:mi>h</m:mi>
                                <m:mn>2</m:mn>
                              </m:msub>
                              <m:msub>
                                <m:mi>ρ</m:mi>
