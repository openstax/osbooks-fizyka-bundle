                              <m:mspace width="0.2em"/>
                              <m:mrow>
                                <m:mi mathvariant="normal">m</m:mi>
                                <m:mspace width="0.1em"/><m:mtext>/</m:mtext><m:mspace width="0.1em"/>
                                <m:mi mathvariant="normal">s</m:mi>
                              </m:mrow>
                            </m:mrow>
                            <m:mo>)</m:mo>
                          </m:mrow>
                          <m:mn>2</m:mn>
                        </m:msup>
                        <m:mo>=</m:mo>
                        <m:mn>84,4</m:mn>
                        <m:mspace width="0.2em"/>
                        <m:mrow>
                          <m:mi mathvariant="normal">J</m:mi>
                        </m:mrow>
                        <m:mo>,</m:mo>
                      </m:math>
                    </item>
                    <item>
                      <m:math>
                        <m:msub>
                          <m:mi>v</m:mi>
                          <m:mtext>CT</m:mtext>
                        </m:msub>
                        <m:mo>=</m:mo>
                        <m:mn>15,0</m:mn>
                        <m:mspace width="0.2em"/>
                        <m:mrow>
                          <m:mi mathvariant="normal">m</m:mi>
                          <m:mspace width="0.1em"/><m:mtext>/</m:mtext><m:mspace width="0.1em"/>
                          <m:mi mathvariant="normal">s</m:mi>
                        </m:mrow>
                        <m:mo>±</m:mo>
                        <m:mn>1,50</m:mn>
                        <m:mspace width="0.2em"/>
                        <m:mrow>
                          <m:mi mathvariant="normal">m</m:mi>
                          <m:mspace width="0.1em"/><m:mtext>/</m:mtext><m:mspace width="0.1em"/>
                          <m:mi mathvariant="normal">s</m:mi>
                        </m:mrow>
                        <m:mo>.</m:mo>
                      </m:math> W związku z tym mamy dwie poprawne odpowiedzi:<newline/>
                      <equation id="fs-id1165037981655" class="unnumbered">
                        <label/>
                        <m:math>
                          <m:mi>E</m:mi>
                          <m:mo>=</m:mo>
                          <m:mfrac>
                            <m:mn>1</m:mn>
                            <m:mn>2</m:mn>
                          </m:mfrac>
                          <m:mo>⋅</m:mo>
                          <m:mn>75,0</m:mn>
                          <m:mspace width="0.2em"/>
                          <m:mrow>
                            <m:mi mathvariant="normal">k</m:mi>
                            <m:mi mathvariant="normal">g</m:mi>
                          </m:mrow>
                          <m:mo>⋅</m:mo>
                          <m:msup>
                            <m:mrow>
                              <m:mo>(</m:mo>
                              <m:mrow>
                                <m:mn>13,5</m:mn>
                                <m:mspace width="0.2em"/>
                                <m:mrow>
                                  <m:mi mathvariant="normal">m</m:mi>
                                  <m:mspace width="0.1em"/><m:mtext>/</m:mtext><m:mspace width="0.1em"/>
                                  <m:mi mathvariant="normal">s</m:mi>
                                </m:mrow>
                              </m:mrow>
                              <m:mo>)</m:mo>
                            </m:mrow>
                            <m:mn>2</m:mn>
                          </m:msup>
                          <m:mo>=</m:mo>
                          <m:mn>6,83</m:mn>
                          <m:mspace width="0.2em"/>
                          <m:mrow>
                            <m:mi mathvariant="normal">k</m:mi>
                            <m:mi mathvariant="normal">J</m:mi>
                          </m:mrow>
                          <m:mo>,</m:mo>
                        </m:math>
                      </equation>
                      <newline/>i<newline/>
                      <equation id="fs-id1165038046576" class="unnumbered">
                        <label/>
                        <m:math>
                          <m:mi>E</m:mi>
                          <m:mo>=</m:mo>
                          <m:mfrac>
                            <m:mn>1</m:mn>
                            <m:mn>2</m:mn>
                          </m:mfrac>
                          <m:mo>⋅</m:mo>
                          <m:mn>75,0</m:mn>
                          <m:mspace width="0.2em"/>
                          <m:mrow>
                            <m:mi mathvariant="normal">k</m:mi>
                            <m:mi mathvariant="normal">g</m:mi>
                          </m:mrow>
                          <m:mo>⋅</m:mo>
                          <m:msup>
                            <m:mrow>
                              <m:mo>(</m:mo>
                              <m:mrow>
                                <m:mn>16,5</m:mn>
                                <m:mspace width="0.2em"/>
                                <m:mrow>
                                  <m:mi mathvariant="normal">m</m:mi>
                                  <m:mspace width="0.1em"/><m:mtext>/</m:mtext><m:mspace width="0.1em"/>
                                  <m:mi mathvariant="normal">s</m:mi>
                                </m:mrow>
                              </m:mrow>
                              <m:mo>)</m:mo>
                            </m:mrow>
                            <m:mn>2</m:mn>
                          </m:msup>
                          <m:mo>=</m:mo>
                          <m:mn>10,2</m:mn>
                          <m:mspace width="0.2em"/>
                          <m:mrow>
                            <m:mi mathvariant="normal">k</m:mi>
                            <m:mi mathvariant="normal">J</m:mi>
