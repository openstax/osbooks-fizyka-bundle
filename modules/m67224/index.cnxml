<glossary>
  <definition id="fs-id1167134468806">
    <term xmlns:cmlnle="http://katalysteducation.org/cmlnle/1.0" class="no-emphasis" cmlnle:reference="henr">henr (ang. <foreign><term xmlns:cxlxt="http://katalysteducation.org/cxlxt/1.0" cxlxt:index="foreign" class="no-emphasis">henry</term></foreign>)</term>
      <meaning id="fs-id1167134991822">jednostka indukcyjności, <m:math display="inline">
        <m:mrow>
          <m:mn>1</m:mn>
         <m:mo rspace="verythinmathspace" lspace="">⁢</m:mo>
          <m:mi mathvariant="normal">H</m:mi>
        </m:mrow>
        <m:mo>=</m:mo>
        <m:mrow>
          <m:mn>1</m:mn>
         <m:mo rspace="verythinmathspace" lspace="">⁢</m:mo>
          <m:mrow>
            <m:mi mathvariant="normal">Ω</m:mi>
           <m:mo rspace="verythinmathspace" lspace="">⁢</m:mo>
            <m:mi mathvariant="normal">s</m:mi>
          </m:mrow>
        </m:mrow>
      </m:math>
    </meaning>
  </definition>
  <definition id="fs-id1167134588330">
    <term xmlns:cmlnle="http://katalysteducation.org/cmlnle/1.0" class="no-emphasis" cmlnle:reference="indukcyjność">indukcyjność (ang. <foreign><term xmlns:cxlxt="http://katalysteducation.org/cxlxt/1.0" cxlxt:index="foreign" class="no-emphasis">inductance</term></foreign>)</term>
      <meaning id="fs-id1167130027871">własność urządzenia określająca jego zdolność do indukowania SEM w innym urządzeniu</meaning>
    </definition>
    <definition id="fs-id1167131237741">
      <term xmlns:cmlnle="http://katalysteducation.org/cmlnle/1.0" class="no-emphasis" cmlnle:reference="indukcyjność wzajemna">indukcyjność wzajemna (ang. <foreign><term xmlns:cxlxt="http://katalysteducation.org/cxlxt/1.0" cxlxt:index="foreign" class="no-emphasis">mutual inductance</term></foreign>)</term>
        <meaning id="fs-id1167131363298">geometryczna wielkość określająca zdolność dwóch urządzeń do wzajemnego indukowania SEM</meaning>
      </definition>
    </glossary>
  </document>