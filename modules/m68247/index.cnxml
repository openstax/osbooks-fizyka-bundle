                        </m:mrow>
                        <m:mo>+</m:mo>
                        <m:mrow>
                          <m:mi>V</m:mi>
                          <m:mo>⁡</m:mo>
                          <m:mfenced>
                            <m:mi>x</m:mi>
                            <m:mi>t</m:mi>
                          </m:mfenced>
                        </m:mrow>
                      </m:mrow>
                    </m:mfenced>
                    <m:mo>⁢</m:mo>
                    <m:mrow>
                      <m:mi mathvariant="normal">Ψ</m:mi>
                      <m:mo>⁡</m:mo>
                      <m:mfenced>
                        <m:mi>x</m:mi>
                        <m:mi>t</m:mi>
                      </m:mfenced>
                    </m:mrow>
                  </m:mrow>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd>
                  <m:mphantom>
                    <m:mrow>
                      <m:mi>H</m:mi>
