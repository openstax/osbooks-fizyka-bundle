                      </m:mfenced>
                    </m:mrow>
                  </m:mrow>
                  <m:mo>=</m:mo>
                  <m:mrow>
                    <m:mfenced close="]" open="[">
                      <m:mrow>
                        <m:mrow>
                          <m:mo>−</m:mo>
                          <m:mrow>
                            <m:mfrac>
                              <m:msup>
                                <m:mi>ℏ</m:mi>
                                <m:mn>2</m:mn>
                              </m:msup>
                              <m:mrow>
                                <m:mn>2</m:mn>
                                <m:mo>⁢</m:mo>
                                <m:mi>m</m:mi>
                              </m:mrow>
                            </m:mfrac>
                            <m:mo>⋅</m:mo>
                            <m:mfrac>
                              <m:msup>
                                <m:mo>∂</m:mo>
                                <m:mn>2</m:mn>
                              </m:msup>
                              <m:mrow>
                                <m:mo>∂</m:mo>
                                <m:msup>
                                  <m:mi>x</m:mi>
                                  <m:mn>2</m:mn>
