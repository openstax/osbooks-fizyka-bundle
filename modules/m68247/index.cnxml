                        </m:mfrac>
                        <m:mrow>
                          <m:mi mathvariant="normal">Ψ</m:mi>
                          <m:mo>⁡</m:mo>
                          <m:mfenced>
                            <m:mi>x</m:mi>
                            <m:mi>t</m:mi>
                          </m:mfenced>
                        </m:mrow>
                      </m:mrow>
                    </m:mrow>
                  </m:mrow>
                  <m:mo>=</m:mo>
                  <m:mrow>
                    <m:mfenced close="]" open="[">
                      <m:mrow>
                        <m:mrow>
                          <m:mfrac>
                            <m:mn>1</m:mn>
                            <m:mrow>
                              <m:mn>2</m:mn>
                              <m:mo>⁢</m:mo>
                              <m:mi>m</m:mi>
                            </m:mrow>
                          </m:mfrac>
                          <m:mo>⁢</m:mo>
                          <m:msup>
                            <m:mfenced>
                              <m:mrow>
                                <m:mo>−</m:mo>
                                <m:mrow>
