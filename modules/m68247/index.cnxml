                        </m:mfenced>
                      </m:mrow>
                    </m:mrow>
                  </m:mphantom>
                  <m:mo id="U93eab039-13bd-4ba5-984c-61bf59d3bbe6">=</m:mo>
                  <m:mrow>
                    <m:mfenced close="]" open="[">
                      <m:mrow>
                        <m:mrow>
                          <m:mo>−</m:mo>
                          <m:mrow>
                            <m:mfrac>
                              <m:msup>
                                <m:mi>ℏ</m:mi>
