                                  <m:mo>⁢</m:mo>
                                  <m:mi>ℏ</m:mi>
                                  <m:mo>⁢</m:mo>
                                  <m:mfrac>
                                    <m:mo>∂</m:mo>
                                    <m:mrow>
                                      <m:mo>∂</m:mo>
                                      <m:mi>x</m:mi>
                                    </m:mrow>
                                  </m:mfrac>
                                </m:mrow>
