                                              </m:mover>
                                            </m:mtd>
                                          </m:mtr>
                                          <m:mtr>
                                            <m:mtd>
                                              <m:mover>
                                                <m:mi>j</m:mi>
                                                <m:mo stretchy="false">^</m:mo>
                                              </m:mover>
                                              <m:mo>×</m:mo>
                                              <m:mover>
                                                <m:mi>k</m:mi>
                                                <m:mo stretchy="false">^</m:mo>
                                              </m:mover>
                                              <m:mo>=</m:mo>
                                              <m:mo>+</m:mo>
                                              <m:mover>
                                                <m:mi>i</m:mi>
                                                <m:mo stretchy="false">^</m:mo>
                                              </m:mover>
                                            </m:mtd>
                                          </m:mtr>
                                          <m:mtr>
                                            <m:mtd>
                                              <m:mover>
                                                <m:mi>k</m:mi>
                                                <m:mo stretchy="false">^</m:mo>
                                              </m:mover>
                                              <m:mo>×</m:mo>
                                              <m:mover>
                                                <m:mi>i</m:mi>
                                                <m:mo stretchy="false">^</m:mo>
                                              </m:mover>
                                              <m:mo>=</m:mo>
                                              <m:mo>+</m:mo>
                                              <m:mover>
                                                <m:mi>j</m:mi>
                                                <m:mo stretchy="false">^</m:mo>
