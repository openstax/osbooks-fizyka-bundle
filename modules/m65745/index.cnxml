                                                                                                                                                                                                                                    <m:mi>G</m:mi>
                                                                                                                                                                                                                                    <m:mtext>→</m:mtext>
                                                                                                                                                                                                                                  </m:mover>
                                                                                                                                                                                                                                </m:mrow>
                                                                                                                                                                                                                                <m:mo>×</m:mo>
                                                                                                                                                                                                                                <m:mrow>
                                                                                                                                                                                                                                  <m:mover>
                                                                                                                                                                                                                                    <m:mi>H</m:mi>
                                                                                                                                                                                                                                    <m:mtext>→</m:mtext>
                                                                                                                                                                                                                                  </m:mover>
                                                                                                                                                                                                                                </m:mrow>
                                                                                                                                                                                                                              </m:mrow>
                                                                                                                                                                                                                              <m:mo>|</m:mo>
