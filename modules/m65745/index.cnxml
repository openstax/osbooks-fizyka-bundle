                                  <row>
                                    <entry align="left">Iloczyn skalarny obliczany przy pomocy poszczególnych składowych wektorów</entry>
                                    <entry align="left">
                                      <m:math>
                                        <m:mover>
                                          <m:mi>A</m:mi>
                                          <m:mtext>→</m:mtext>
                                        </m:mover>
                                        <m:mo>·</m:mo>
                                        <m:mover>
                                          <m:mi>B</m:mi>
                                          <m:mtext>→</m:mtext>
                                        </m:mover>
                                        <m:mo>=</m:mo>
                                        <m:msub>
                                          <m:mi>A</m:mi>
                                          <m:mi>x</m:mi>
                                        </m:msub>
                                        <m:msub>
                                          <m:mi>B</m:mi>
                                          <m:mi>x</m:mi>
                                        </m:msub>
                                        <m:mo>+</m:mo>
                                        <m:msub>
                                          <m:mi>A</m:mi>
                                          <m:mi>y</m:mi>
                                        </m:msub>
                                        <m:msub>
                                          <m:mi>B</m:mi>
                                          <m:mi>y</m:mi>
                                        </m:msub>
                                        <m:mo>+</m:mo>
                                        <m:msub>
                                          <m:mi>A</m:mi>
                                          <m:mi>z</m:mi>
                                        </m:msub>
                                        <m:msub>
                                          <m:mi>B</m:mi>
                                          <m:mi>z</m:mi>
