                                  <row>
                                    <entry align="left">Cosinus kąta między dwoma wektorami</entry>
                                    <entry align="left">
                                      <m:math>
                                        <m:mtext>cos</m:mtext>
                                        <m:mspace width="0.2em"/>
                                        <m:mi>φ</m:mi>
                                        <m:mo>=</m:mo>
                                        <m:mfrac>
                                          <m:mrow>
                                            <m:mover>
                                              <m:mi>A</m:mi>
                                              <m:mtext>→</m:mtext>
                                            </m:mover>
                                            <m:mo>·</m:mo>
                                            <m:mover>
                                              <m:mi>B</m:mi>
                                              <m:mtext>→</m:mtext>
                                            </m:mover>
                                          </m:mrow>
                                          <m:mrow>
                                            <m:mi>A</m:mi>
                                            <m:mi>B</m:mi>
                                          </m:mrow>
                                        </m:mfrac>
                                      </m:math>
                                    </entry>
                                  </row>
                                  <row>
                                    <entry align="left">Iloczyn skalarny wektorów jednostkowych</entry>
                                    <entry align="left">
                                      <m:math>
                                        <m:mover>
                                          <m:mi>i</m:mi>
                                          <m:mo stretchy="false">^</m:mo>
                                        </m:mover>
                                        <m:mo>·</m:mo>
                                        <m:mover>
                                          <m:mi>j</m:mi>
                                          <m:mo stretchy="false">^</m:mo>
                                        </m:mover>
                                        <m:mo>=</m:mo>
                                        <m:mover>
                                          <m:mi>j</m:mi>
                                          <m:mo stretchy="false">^</m:mo>
                                        </m:mover>
                                        <m:mo>·</m:mo>
                                        <m:mover>
                                          <m:mi>k</m:mi>
                                          <m:mo stretchy="false">^</m:mo>
                                        </m:mover>
                                        <m:mo>=</m:mo>
                                        <m:mover>
                                          <m:mi>k</m:mi>
                                          <m:mo stretchy="false">^</m:mo>
                                        </m:mover>
                                        <m:mo>·</m:mo>
                                        <m:mover>
                                          <m:mi>i</m:mi>
                                          <m:mo stretchy="false">^</m:mo>
                                        </m:mover>
                                        <m:mo>=</m:mo>
                                        <m:mn>0</m:mn>
                                      </m:math>
                                    </entry>
                                  </row>
                                  <row>
                                    <entry align="left">Moduł iloczynu wektorowego (definicja)</entry>
                                    <entry align="left">
                                      <m:math>
                                        
                                      </m:math>
                                      <m:math>
                                        <m:mrow>
                                          <m:mo>|</m:mo>
                                          <m:mrow>
                                            <m:mrow>
                                              <m:mover>
                                                <m:mi>A</m:mi>
                                                <m:mtext>→</m:mtext>
                                              </m:mover>
                                            </m:mrow>
                                            <m:mo>×</m:mo>
                                            <m:mrow>
                                              <m:mover>
                                                <m:mi>B</m:mi>
                                                <m:mtext>→</m:mtext>
                                              </m:mover>
                                            </m:mrow>
                                          </m:mrow>
                                          <m:mo>|</m:mo>
                                        </m:mrow>
                                        <m:mo>=</m:mo>
                                        <m:mi>A</m:mi>
                                        <m:mi>B</m:mi>
                                        <m:mi>sin</m:mi>
                                        <m:mo>⁡</m:mo>
                                        <m:mi>φ</m:mi>
                                      </m:math>
                                    </entry>
                                  </row>
                                  <row>
                                    <entry align="left">Nieprzemienność iloczynu wektorowego</entry>
                                    <entry align="left">
                                      <m:math>
                                        <m:mrow>
                                          <m:mover>
                                            <m:mi>A</m:mi>
                                            <m:mtext>→</m:mtext>
                                          </m:mover>
                                        </m:mrow>
                                        <m:mo>×</m:mo>
                                        <m:mrow>
                                          <m:mover>
                                            <m:mi>B</m:mi>
                                            <m:mtext>→</m:mtext>
                                          </m:mover>
                                        </m:mrow>
                                        <m:mo>=</m:mo>
                                        <m:mo>−</m:mo>
                                        <m:mrow>
                                          <m:mover>
                                            <m:mi>B</m:mi>
                                            <m:mtext>→</m:mtext>
                                          </m:mover>
                                        </m:mrow>
                                        <m:mo>×</m:mo>
                                        <m:mrow>
                                          <m:mover>
                                            <m:mi>A</m:mi>
                                            <m:mtext>→</m:mtext>
                                          </m:mover>
