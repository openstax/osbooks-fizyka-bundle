                                    <entry align="left">
                                      <m:math>
                                        <m:mo>{</m:mo>
                                        <m:mtable columnalign="left">
                                          <m:mtr>
                                            <m:mtd>
                                              <m:mover>
                                                <m:mi>i</m:mi>
                                                <m:mo stretchy="false">^</m:mo>
