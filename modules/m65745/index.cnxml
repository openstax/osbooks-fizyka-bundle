                                    </entry>
                                  </row>
                                  <row>
                                    <entry align="left">Definicja iloczynu skalarnego</entry>
                                    <entry align="left">
                                      <m:math>
                                        <m:mover>
                                          <m:mi>A</m:mi>
                                          <m:mtext>→</m:mtext>
                                        </m:mover>
                                        <m:mo>·</m:mo>
                                        <m:mover>
                                          <m:mi>B</m:mi>
                                          <m:mtext>→</m:mtext>
                                        </m:mover>
                                        <m:mo>=</m:mo>
                                        <m:mi>A</m:mi>
                                        <m:mi>B</m:mi>
                                        <m:mspace width="0.2em"/>
                                        <m:mtext>cos</m:mtext>
                                        <m:mspace width="0.2em"/>
                                        <m:mi>φ</m:mi>
                                      </m:math>
                                    </entry>
                                  </row>
                                  <row>
                                    <entry align="left">Przemienność iloczynu skalarnego</entry>
                                    <entry align="left">
                                      <m:math>
                                        <m:mrow>
                                          <m:mover>
                                            <m:mi>A</m:mi>
                                            <m:mtext>→</m:mtext>
                                          </m:mover>
                                        </m:mrow>
                                        <m:mo>⋅</m:mo>
                                        <m:mrow>
                                          <m:mover>
                                            <m:mi>B</m:mi>
                                            <m:mtext>→</m:mtext>
                                          </m:mover>
                                        </m:mrow>
                                        <m:mo>=</m:mo>
                                        <m:mrow>
                                          <m:mover>
                                            <m:mi>B</m:mi>
                                            <m:mtext>→</m:mtext>
                                          </m:mover>
                                        </m:mrow>
                                        <m:mo>⋅</m:mo>
                                        <m:mrow>
                                          <m:mover>
                                            <m:mi>A</m:mi>
                                            <m:mtext>→</m:mtext>
                                          </m:mover>
                                        </m:mrow>
                                      </m:math>
                                    </entry>
                                  </row>
                                  <row>
                                    <entry align="left">Rozdzielność iloczynu skalarnego względem dodawania</entry>
                                    <entry align="left">
                                      <m:math>
                                        
                                      </m:math>
                                      <m:math>
                                        <m:mrow>
                                          <m:mover>
                                            <m:mi>A</m:mi>
                                            <m:mtext>→</m:mtext>
                                          </m:mover>
                                        </m:mrow>
                                        <m:mo>⋅</m:mo>
                                        <m:mrow>
                                          <m:mo>(</m:mo>
                                          <m:mrow>
                                            <m:mrow>
                                              <m:mover>
                                                <m:mi>B</m:mi>
                                                <m:mtext>→</m:mtext>
                                              </m:mover>
                                            </m:mrow>
                                            <m:mo>+</m:mo>
                                            <m:mrow>
                                              <m:mover>
                                                <m:mi>C</m:mi>
                                                <m:mtext>→</m:mtext>
                                              </m:mover>
                                            </m:mrow>
                                          </m:mrow>
                                          <m:mo>)</m:mo>
                                        </m:mrow>
                                        <m:mo>=</m:mo>
                                        <m:mrow>
                                          <m:mover>
                                            <m:mi>A</m:mi>
                                            <m:mtext>→</m:mtext>
                                          </m:mover>
                                        </m:mrow>
                                        <m:mo>⋅</m:mo>
                                        <m:mrow>
                                          <m:mover>
                                            <m:mi>B</m:mi>
                                            <m:mtext>→</m:mtext>
                                          </m:mover>
                                        </m:mrow>
                                        <m:mo>+</m:mo>
                                        <m:mrow>
                                          <m:mover>
                                            <m:mi>A</m:mi>
                                            <m:mtext>→</m:mtext>
                                          </m:mover>
                                        </m:mrow>
                                        <m:mo>⋅</m:mo>
                                        <m:mrow>
                                          <m:mover>
                                            <m:mi>C</m:mi>
