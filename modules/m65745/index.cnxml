                                                                                                                                                                                                                        <m:mrow>
                                                                                                                                                                                                                          <m:mover>
                                                                                                                                                                                                                            <m:mi>G</m:mi>
                                                                                                                                                                                                                            <m:mtext>→</m:mtext>
                                                                                                                                                                                                                          </m:mover>
                                                                                                                                                                                                                        </m:mrow>
