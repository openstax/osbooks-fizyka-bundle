                                  <row>
                                    <entry align="left">Rozdzielność iloczynu wektorowego względem dodawania</entry>
                                    <entry align="left">
                                      <m:math>
                                        
                                      </m:math>
                                      <m:math>
                                        <m:mrow>
                                          <m:mover>
                                            <m:mi>A</m:mi>
                                            <m:mtext>→</m:mtext>
                                          </m:mover>
                                        </m:mrow>
                                        <m:mo>×</m:mo>
                                        <m:mrow>
                                          <m:mo>(</m:mo>
                                          <m:mrow>
                                            <m:mrow>
                                              <m:mover>
                                                <m:mi>B</m:mi>
                                                <m:mtext>→</m:mtext>
                                              </m:mover>
                                            </m:mrow>
                                            <m:mo>+</m:mo>
                                            <m:mrow>
                                              <m:mover>
                                                <m:mi>C</m:mi>
                                                <m:mtext>→</m:mtext>
                                              </m:mover>
                                            </m:mrow>
                                          </m:mrow>
                                          <m:mo>)</m:mo>
                                        </m:mrow>
                                        <m:mo>=</m:mo>
                                        <m:mrow>
                                          <m:mover>
                                            <m:mi>A</m:mi>
                                            <m:mtext>→</m:mtext>
                                          </m:mover>
                                        </m:mrow>
                                        <m:mo>×</m:mo>
                                        <m:mrow>
                                          <m:mover>
                                            <m:mi>B</m:mi>
                                            <m:mtext>→</m:mtext>
                                          </m:mover>
                                        </m:mrow>
                                        <m:mo>+</m:mo>
                                        <m:mrow>
                                          <m:mover>
                                            <m:mi>A</m:mi>
                                            <m:mtext>→</m:mtext>
                                          </m:mover>
                                        </m:mrow>
                                        <m:mo>×</m:mo>
                                        <m:mrow>
                                          <m:mover>
                                            <m:mi>C</m:mi>
