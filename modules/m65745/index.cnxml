                                                                                                                                                                                                                        </m:mover>
                                                                                                                                                                                                                      </m:mrow>
                                                                                                                                                                                                                    </m:math>
                                                                                                                                                                                                                  </para>
                                                                                                                                                                                                                </solution>
                                                                                                                                                                                                              </exercise>
                                                                                                                                                                                                              <exercise id="fs-id1167134886863">
                                                                                                                                                                                                                <problem id="fs-id1167134886865">
