            </m:mrow>
          </m:mrow>
        </m:mtd>
      </m:mtr>
    </m:mtable>
  </m:math>
</equation>
<para id="paragraph-15">Dokonajmy podstawienia <m:math display="inline">
  <m:mi>u</m:mi>
  <m:mo>=</m:mo>
  <m:mrow>
    <m:mrow>
      <m:mi>ε</m:mi>
      <m:mo>⁢</m:mo>
      <m:mi>C</m:mi>
    </m:mrow>
    <m:mo>−</m:mo>
    <m:mi>q</m:mi>
  </m:mrow>
</m:math>, więc <m:math display="inline">
<m:mrow>
  <m:mo rspace="veryverythinmathspace" lspace="verythinmathspace">d</m:mo>
  <m:mi>u</m:mi>
</m:mrow>
<m:mo>=</m:mo>
<m:mrow>
  <m:mo>−</m:mo>
  <m:mrow>
    <m:mo rspace="veryverythinmathspace" lspace="verythinmathspace">d</m:mo>
    <m:mi>q</m:mi>
  </m:mrow>
</m:mrow>
</m:math>. Dostajemy </para>
<equation id="imp-idm787549280" class="unnumbered">
  <label/>
  <m:math display="block">
    <m:mtable>
      <m:mtr>
        <m:mtd>
          <m:mrow>
            <m:mo>−</m:mo>
            <m:mrow>
              <m:munderover>
                <m:mo>∫</m:mo>
                <m:mn>0</m:mn>
                <m:mn>1</m:mn>
              </m:munderover>
              <m:mfrac>
                <m:mrow>
                  <m:mo rspace="veryverythinmathspace" lspace="verythinmathspace">d</m:mo>
                  <m:mi>u</m:mi>
                </m:mrow>
                <m:mi>u</m:mi>
              </m:mfrac>
              <m:maligngroup/>
            </m:mrow>
          </m:mrow>
          <m:mo>=</m:mo>
          <m:mrow>
            <m:mfrac>
              <m:mn>1</m:mn>
              <m:mrow>
                <m:mi>R</m:mi>
                <m:mo>⁢</m:mo>
                <m:mi>C</m:mi>
              </m:mrow>
            </m:mfrac>
            <m:mo>⁢</m:mo>
            <m:mrow>
              <m:munderover>
                <m:mo>∫</m:mo>
                <m:mn>0</m:mn>
                <m:mi>t</m:mi>
              </m:munderover>
              <m:mrow>
                <m:mo rspace="veryverythinmathspace" lspace="verythinmathspace">d</m:mo>
                <m:mrow>
                  <m:mi>t</m:mi>
                  <m:mo>⁢</m:mo>
                  <m:mtext>,</m:mtext>
                </m:mrow>
              </m:mrow>
            </m:mrow>
          </m:mrow>
        </m:mtd>
      </m:mtr>
      <m:mtr>
        <m:mtd>
          <m:mrow>
            <m:mo>ln</m:mo>
            <m:mfenced>
              <m:mfrac>
                <m:mrow>
                  <m:mrow>
                    <m:mi>ε</m:mi>
                    <m:mo>⁢</m:mo>
                    <m:mi>C</m:mi>
                  </m:mrow>
                  <m:mo>−</m:mo>
                  <m:mi>q</m:mi>
                </m:mrow>
                <m:mrow>
                  <m:mi>ε</m:mi>
                  <m:mo>⁢</m:mo>
                  <m:mi>C</m:mi>
                </m:mrow>
              </m:mfrac>
            </m:mfenced>
            <m:maligngroup/>
          </m:mrow>
          <m:mo>=</m:mo>
          <m:mrow>
            <m:mo>−</m:mo>
            <m:mrow>
              <m:mfrac>
                <m:mn>1</m:mn>
                <m:mrow>
                  <m:mi>R</m:mi>
                  <m:mo>⁢</m:mo>
                  <m:mi>C</m:mi>
                </m:mrow>
              </m:mfrac>
              <m:mo>⁢</m:mo>
              <m:mi>t</m:mi>
              <m:mo>⁢</m:mo>
              <m:mtext>,</m:mtext>
            </m:mrow>
          </m:mrow>
        </m:mtd>
      </m:mtr>
      <m:mtr>
        <m:mtd>
          <m:mfrac>
            <m:mrow>
              <m:mrow>
                <m:mi>ε</m:mi>
                <m:mo>⁢</m:mo>
                <m:mi>C</m:mi>
              </m:mrow>
              <m:mo>−</m:mo>
              <m:mi>q</m:mi>
            </m:mrow>
            <m:mrow>
              <m:mi>ε</m:mi>
              <m:mo>⁢</m:mo>
              <m:mi>C</m:mi>
            </m:mrow>
          </m:mfrac>
          <m:maligngroup/>
          <m:mo>=</m:mo>
          <m:mrow>
            <m:msup>
              <m:mi>e</m:mi>
