      </m:msub>
      <m:mo>=</m:mo>
      <m:msub>
        <m:mrow>
          <m:mrow>
            <m:mover>
              <m:mi>v</m:mi>
              <m:mtext>→</m:mtext>
            </m:mover>
          </m:mrow>
        </m:mrow>
        <m:mrow>
          <m:mtext>M,W</m:mtext>
        </m:mrow>
      </m:msub>
      <m:mo>+</m:mo>
      <m:msub>
        <m:mrow>
          <m:mrow>
            <m:mover>
              <m:mi>v</m:mi>
              <m:mtext>→</m:mtext>
            </m:mover>
          </m:mrow>
        </m:mrow>
        <m:mrow>
          <m:mtext>W,Z</m:mtext>
        </m:mrow>
      </m:msub>
      <m:mo stretchy="false">⇒</m:mo>
      <m:msub>
        <m:mrow>
          <m:mrow>
            <m:mover>
              <m:mi>v</m:mi>
              <m:mtext>→</m:mtext>
            </m:mover>
          </m:mrow>
        </m:mrow>
        <m:mrow>
          <m:mtext>W,Z</m:mtext>
        </m:mrow>
      </m:msub>
      <m:mo>=</m:mo>
      <m:msub>
        <m:mrow>
          <m:mrow>
            <m:mover>
              <m:mi>v</m:mi>
              <m:mtext>→</m:mtext>
            </m:mover>
          </m:mrow>
        </m:mrow>
        <m:mrow>
          <m:mtext>M,Z</m:mtext>
        </m:mrow>
      </m:msub>
      <m:mo>−</m:mo>
      <m:msub>
        <m:mrow>
          <m:mrow>
            <m:mover>
              <m:mi>v</m:mi>
              <m:mtext>→</m:mtext>
            </m:mover>
          </m:mrow>
        </m:mrow>
        <m:mrow>
