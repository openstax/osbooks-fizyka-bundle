            </m:mover>
          </m:mrow>
        </m:mrow>
        <m:mrow>
          <m:mtext>W,Z</m:mtext>
        </m:mrow>
      </m:msub>
      <m:mo>=</m:mo>
      <m:mo>?</m:mo>
      <m:mspace width="1em"/>
      <m:msub>
        <m:mrow>
          <m:mrow>
            <m:mover>
              <m:mi>v</m:mi>
              <m:mtext>→</m:mtext>
            </m:mover>
          </m:mrow>
        </m:mrow>
        <m:mrow>
