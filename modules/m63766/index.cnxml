              <m:mn>,</m:mn>
              <m:mspace width="1em"/>
              <m:msub>
                <m:mi>v</m:mi>
                <m:mi>y</m:mi>
              </m:msub>
              <m:mrow>
                <m:mo>(</m:mo>
                <m:mi>t</m:mi>
                <m:mo>)</m:mo>
              </m:mrow>
              <m:mo>=</m:mo>
              <m:mfrac>
                <m:mrow>
                  <m:mtext>d</m:mtext>
                  <m:mi>y</m:mi>
                  <m:mrow>
                    <m:mo>(</m:mo>
                    <m:mi>t</m:mi>
                    <m:mo>)</m:mo>
                  </m:mrow>
                </m:mrow>
                <m:mrow>
                  <m:mtext>d</m:mtext>
                  <m:mi>t</m:mi>
                </m:mrow>
              </m:mfrac>
              <m:mn>,</m:mn>
              <m:mspace width="1em"/>
              <m:msub>
                <m:mi>v</m:mi>
                <m:mi>z</m:mi>
              </m:msub>
              <m:mrow>
                <m:mo>(</m:mo>
                <m:mi>t</m:mi>
                <m:mo>)</m:mo>
              </m:mrow>
              <m:mo>=</m:mo>
              <m:mfrac>
                <m:mrow>
                  <m:mtext>d</m:mtext>
