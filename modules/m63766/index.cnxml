    </m:math>
    <newline/>Zatem prędkość wiatru względem ziemi: <m:math>
    <m:msub>
            <m:mi>v</m:mi>
      <m:mrow>
        <m:mtext>W,Z</m:mtext>
      </m:mrow>
    </m:msub>
    <m:mo>=</m:mo>
    <m:mo>−</m:mo>
    <m:mn>4,0</m:mn>
    <m:mspace width="0.2em"/>
    <m:mtext>m/s</m:mtext>
    </m:math>.</item>
    <item>
      <m:math>
        <m:msub>
          <m:mrow>
            <m:mrow>
              <m:mover>
                <m:mi>v</m:mi>
                <m:mtext>→</m:mtext>
              </m:mover>
            </m:mrow>
          </m:mrow>
          <m:mrow>
            <m:mtext>M,Z</m:mtext>
          </m:mrow>
        </m:msub>
        <m:mo>=</m:mo>
        <m:msub>
          <m:mrow>
            <m:mrow>
              <m:mover>
                <m:mi>v</m:mi>
                <m:mtext>→</m:mtext>
              </m:mover>
            </m:mrow>
          </m:mrow>
          <m:mrow>
            <m:mtext>M,W</m:mtext>
          </m:mrow>
        </m:msub>
        <m:mo>+</m:mo>
        <m:msub>
          <m:mrow>
            <m:mrow>
              <m:mover>
                <m:mi>v</m:mi>
                <m:mtext>→</m:mtext>
              </m:mover>
            </m:mrow>
          </m:mrow>
          <m:mrow>
            <m:mtext>W,Z</m:mtext>
          </m:mrow>
        </m:msub>
        <m:mo stretchy="false">⇒</m:mo>
        <m:msub>
                <m:mi>v</m:mi>
          <m:mrow>
            <m:mtext>M,Z</m:mtext>
          </m:mrow>
        </m:msub>
        <m:mo>=</m:mo>
        <m:mo>−</m:mo>
        <m:mn>13,0</m:mn>
        <m:mspace width="0.2em"/>
        <m:mtext>m/s</m:mtext>
