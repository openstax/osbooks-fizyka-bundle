      </m:msub>
      <m:mo>=</m:mo>
      <m:mn>5</m:mn>
      <m:mspace width="0.2em"/>
      <m:mtext>m/s</m:mtext>
      <m:mn>,</m:mn>
    </m:math>
    <m:math>
      <m:msub>
        <m:mrow>
          <m:mrow>
            <m:mover>
              <m:mi>v</m:mi>
              <m:mtext>→</m:mtext>
            </m:mover>
          </m:mrow>
        </m:mrow>
        <m:mrow>
          <m:mtext>M,Z</m:mtext>
