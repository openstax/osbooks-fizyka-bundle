            <m:mn>2</m:mn>
          </m:mfrac><m:mo>⁢</m:mo>
          <m:mi>m</m:mi>
          <m:mo>⁢</m:mo>
          <m:msubsup><m:mi>v</m:mi>
            <m:mtext>max</m:mtext>
            <m:mn>2</m:mn>
          </m:msubsup></m:mrow><m:mo>=</m:mo>
        <m:mfrac><m:mrow><m:msup><m:mi>q</m:mi>
              <m:mn>2</m:mn>
            </m:msup><m:mo>⁢</m:mo>
            <m:msup><m:mi>B</m:mi>
