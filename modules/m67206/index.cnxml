        <m:mrow>
          <m:mfrac>
            <m:mn>1</m:mn>
            <m:mn>2</m:mn>
          </m:mfrac>
          <m:mo>⁢</m:mo>
          <m:mi>m</m:mi>
          <m:mo>⁢</m:mo>
          <m:msubsup>
            <m:mi>v</m:mi>
