            <m:mo>⁢</m:mo>
            <m:mi>m</m:mi>
          </m:mrow></m:mfrac><m:mo>=</m:mo>
        <m:mfrac><m:mrow><m:msup><m:mfenced><m:mrow><m:mrow><m:mn>3,2</m:mn>
                    <m:mo>⋅</m:mo>
                    <m:msup><m:mn>10</m:mn>
                      <m:mn>-19</m:mn>
                    </m:msup></m:mrow><m:mo rspace="veryverythinmathspace" lspace="veryverythinmathspace">⁢</m:mo>
                  <m:mi mathvariant="normal">C</m:mi>
                </m:mrow></m:mfenced><m:mn>2</m:mn>
            </m:msup><m:mo>⋅</m:mo>
            <m:msup><m:mfenced><m:mrow><m:mn>1,8</m:mn>
