            <row valign="top">
              <entry valign="top" align="left">Energia fotonu</entry>
              <entry valign="top" align="left">
                <m:math display="inline">
                  <m:semantics>
                    <m:mrow>
                      <m:msub>
                        <m:mi>E</m:mi>
