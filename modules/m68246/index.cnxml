            <row valign="top">
              <entry valign="top" align="left">Prawo Plancka</entry>
              <entry valign="top" align="left">
                <m:math display="inline">
                  <m:semantics>
                    <m:mrow>
                      <m:mrow>
                        <m:mi>I</m:mi>
                        <m:mo>⁡</m:mo>
                        <m:mfenced>
