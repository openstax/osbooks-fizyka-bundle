                <m:math display="inline">
                  <m:semantics>
                    <m:mrow>
                      <m:mrow>
                        <m:mo>Δ</m:mo>
                        <m:mi>E</m:mi>
                      </m:mrow>
                      <m:mo>=</m:mo>
