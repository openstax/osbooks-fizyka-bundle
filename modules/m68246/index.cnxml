                  <m:mfrac>
                    <m:mi>h</m:mi>
                    <m:mi>p</m:mi>
                  </m:mfrac>
                  <m:mo>=</m:mo>
                  <m:mfrac>
                    <m:mi>h</m:mi>
                    <m:msqrt>
                      <m:mn>2</m:mn>
                      <m:mo>⁢</m:mo>
                      <m:msub>
                        <m:mi>m</m:mi>
                        <m:mi>e</m:mi>
                      </m:msub>
                      <m:mo>⁢</m:mo>
                      <m:msub>
                        <m:mi>E</m:mi>
                        <m:mtext>k</m:mtext>
                      </m:msub>
                    </m:msqrt>
                  </m:mfrac>
                  <m:mo>=</m:mo>
                  <m:mfrac>
                    <m:mi>h</m:mi>
                    <m:msqrt>
                      <m:mrow>
                        <m:mrow>
                          <m:mn>2</m:mn>
                          <m:mo>⋅</m:mo>
                          <m:msub>
                            <m:mi>E</m:mi>
