                      <m:mi>c</m:mi>
                    </m:mrow>
                    <m:msqrt>
                      <m:mn>2</m:mn>
                      <m:mo>⁢</m:mo>
                      <m:msub>
                        <m:mi>E</m:mi>
                        <m:mn>0</m:mn>
                      </m:msub>
                      <m:mo>⁢</m:mo>
                      <m:msub>
                        <m:mi>E</m:mi>
                        <m:mtext>k</m:mtext>
                      </m:msub>
                    </m:msqrt>
